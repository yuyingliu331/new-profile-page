  <script type="text/javascript" id="gwd-datecode">
    var dateLabels = [{
      date: new Date('8/18/2015'),
      label: 'datecode-friday'
    }, {
      date: new Date('8/28/2015'),
      label: 'datecode-now'
    }];

    function assignDateLabel(data, element) {
      var date = new Date();
      var label = "datecode-date";
      var i = 0,
        l = data.length;

      Utils.removeClass(element, label);

      for (; i < l; i++) {
        if (date >= data[i].date) {
          label = data[i].label;
        }
      }

      Utils.addClass(element, label);
    }

    assignDateLabel(dateLabels, document.getElementsByTagName("body")[0]);
  </script>
  <script type="text/javascript" id="gwd-tracking">
    function dummytrackingfunction() {
      Enabler.exit("DIS300 : Main Exit");
      Enabler.exit("DIS300 : Tickets Exit");

      Enabler.counter("DIS300 : Total Tickets Ctr");

      Enabler.counter("DIS300 : Intro Complete Ctr");
      Enabler.counter("DIS300 : Intro Midpoint Ctr");

      Enabler.counter("DIS300 : Video 1 Play Ctr");
      Enabler.counter("DIS300 : Video 1 Pause Ctr");
      Enabler.counter("DIS300 : Video 1 Unpause Ctr");
      Enabler.counter("DIS300 : Video 1 Complete Ctr");
      Enabler.counter("DIS300 : Video 1 Muted Ctr");
      Enabler.counter("DIS300 : Video 1 Unmuted Ctr");
      Enabler.counter("DIS300 : Video 1 Midpoint Ctr");

      Enabler.counter("DIS300 : Engaged Video View Complete Ctr");
      Enabler.counter("DIS300 : Engaged Video View Midpoint Ctr")
      Enabler.counter("DIS300 : Engaged Video View Ctr");
      Enabler.counter("DIS300 : RM Interactive Impression");

      Enabler.startTimer("DIS300 : Engaged Video View Timer");
      Enabler.stopTimer("DIS300 : Engaged Video View Timer");
      Enabler.startTimer("DIS300 : Intro View Timer");
      Enabler.stopTimer("DIS300 : Intro View Timer");
      Enabler.startTimer("DIS300 : Video View Timer");
      Enabler.stopTimer("DIS300 : Video View Timer");
    }
  </script>
  <script type="text/javascript" id="gwd-init-code">
    (function() {

      var gwdAd = document.getElementById('gwd-ad');

      /** AVATARLABS SPECIFIC CODE - START **/
      CssPointerEvents.init();
      isMobile = Utils.isMobile();
      expandPanel = document.getElementById('expanded-intro-page') ? "expanded-intro-page" : "expanded-video-page";
      document.querySelectorAll('[expanded]').length > 0 ? canExpand = true : canExpand = false;
      Ad.setCanExpand(canExpand);

      TrackingController.setCampaignId("DIS300");
      TrackingController.addTrackingPixel("initial_load", "https://googleads.g.doubleclick.net/pagead/viewthroughconversion/940466018/?value=1.00&currency_code=USD&label=-R9kCLWPs2cQ4r65wAM&guid=ON&script=0");
      TrackingController.addTrackingPixel("click", "https://googleads.g.doubleclick.net/pagead/viewthroughconversion/940466018/?value=1.00&currency_code=USD&label=zinoCOmGyGcQ4r65wAM&guid=ON&script=0");
      Ad.addTrackables(document.getElementsByClassName("track-click"));

      Ad.setContainerClip("pagedeck");
      Ad.setExitCallback(gwd.handleRequestResolvePage);

      YoutubeIntroPlayer.setCompleteCallback(gwd.handleRequestResolvePage);
      YoutubeIntroPlayer.setVideoplayer("intro-page-player");
      YoutubeIntroPlayer.setPreloader("intro-page-preloader");
      YoutubeIntroPlayer.setDivContainer("intro-page-player-container");
      YoutubeIntroPlayer.setSource("intro", "Intro", "6mIjIA3_MFc");

      YoutubeIntroControls.setDiv("intro-page-controls");
      YoutubeIntroControls.setButton("mute", "intro-page-controls-mute");
      YoutubeIntroControls.setButton("unmute", "intro-page-controls-unmute");

      YoutubeVideoPlayer.setCompleteCallback(gwd.handleRequestResolvePage);
      YoutubeVideoPlayer.setVideoplayer("video-page-player");
      YoutubeVideoPlayer.setPreloader("video-page-preloader");
      YoutubeVideoPlayer.setDivContainer("video-page-player-container");
      YoutubeVideoPlayer.setDivContainerOffset("496px");
      YoutubeVideoPlayer.setSource("video1", "Video 1", "pvOTvh34FNE");

      YoutubeVideoControls.setDiv("video-page-controls");
      YoutubeVideoControls.setButton("play", "video-page-controls-play");
      YoutubeVideoControls.setButton("pause", "video-page-controls-pause");
      YoutubeVideoControls.setButton("mute", "video-page-controls-mute");
      YoutubeVideoControls.setButton("unmute", "video-page-controls-unmute");
      YoutubeVideoControls.setScrubberContainer("video-page-controls-scrubber");
      YoutubeVideoControls.setProgressDisplay("video-page-controls-scrubber-progres");
      YoutubeVideoControls.setScrubberTrigger("video-page-controls-scrubber-trigger");

      /** panels **/
      Ad.addPanel("preload", "banner-resolve-page");
      Ad.addPanel("intro", "banner-intro-page");
      Ad.addPanel("video", "banner-video-page");
      Ad.addPanel("resolve", "banner-resolve-page");

      /** fairies **/

      var fairiesContainer = document.getElementById('fairies');
      var fairiesContainerBounds = fairiesContainer.getBoundingClientRect();
      var fairyAnimDuration = 3; // seconds
      var fairyRandomDelay = 1; // seconds
      var numFairies = 14;
      var numFairyTypes = 4;
      var numFairyPathPoints = 3;
      var minFairyScale = 0.15;
      var maxFairyScale = 0.5;
      var diffFairyScale = maxFairyScale - minFairyScale;

      var horizontalDistanceDeviation = 20;
      var horizontalOffset = 74 * 0.5 - horizontalDistanceDeviation * 0.5; // 74 = width / height of unscaled fair /y;
      var horizontalDistance = fairiesContainerBounds.width - horizontalDistanceDeviation;
      // var horizontalDistanceStep = horizontalDistance / numFairies;

      var verticalDistanceDeviation = 20; // px
      var verticalOffset = 74 * 0.5 - verticalDistanceDeviation * 0.5; // 74 = width / height of unscaled fairy;
      var verticalDistance = fairiesContainerBounds.height - verticalDistanceDeviation;
      // var verticalDistanceStep = verticalDistance / numFairyPathPoints;

      var fairiesTL = new TimelineMax({
        paused: true
      });

      for (var i = 0; i < numFairies; i++) {

        fairyConatainer = document.createElement('div');
        fairyConatainer.className = 'fairy-container';

        fairy = document.createElement('div');
        fairy.className = 'fairy fairyType' + parseInt(Math.floor(Math.random() * numFairyTypes) + 1);

        /// generate linear bezierPath
        // var fairyPath = [{ x:horizontalDistanceStep * i - horizontalOffset, y: -verticalOffset }]; /// set init point
        // for( var j = 1; j < numFairyPathPoints; j++ ) {
        //   fairyPath.push({
        //     x: horizontalDistanceStep * i + ( -horizontalDistanceDeviation * 0.5 * Math.random() ) - horizontalOffset,
        //     y: verticalDistanceStep * j + ( -verticalDistanceDeviation * 0.5 + verticalDistanceDeviation * Math.random() )
        //   });
        // }

        /// generate random bezierPath
        var fairyPath = [];
        var baseX = horizontalDistance * Math.random(); /// set init point
        var baseY = verticalDistance * Math.random();
        for (var j = 0; j < numFairyPathPoints; j++) {
          fairyPath.push({
            x: baseX - horizontalDistanceDeviation * 0.5 + horizontalDistanceDeviation * Math.random() - horizontalOffset,
            y: baseY - verticalDistanceDeviation * 0.5 + verticalDistanceDeviation * Math.random() - verticalOffset
          });
        }

        var fairyScale = minFairyScale + diffFairyScale * Math.random();
        var fiaryDelay = 0; //fairyRandomDelay * Math.random();
        var fairyDuration = fairyAnimDuration * 0.5 + fairyAnimDuration * 0.5 * Math.random();

        fairiesTL.add([
          TweenMax.fromTo(fairyConatainer, fairyDuration, {
            x: fairyPath[0].x,
            y: fairyPath[0].y,
            scale: fairyScale,
            immediateRender: true
          }, {
            bezier: fairyPath,
            scale: fairyScale,
            ease: Linear.easeNone
          }), /// follow path
          // TweenMax.fromTo( fairyConatainer, fairyDuration * 0.25, { opacity:1 }, { opacity:0, ease:Linear.easeNone, delay:fairyDuration * 0.75 } ),
          TweenMax.to(fairyConatainer, 0, {
            opacity: 0
          }),
          new TimelineMax({
            tweens: [
              TweenMax.fromTo(fairyConatainer, fairyDuration * 0.2, {
                opacity: 0,
                immediateRender: true
              }, {
                opacity: 1,
                ease: Linear.easeNone
              }),
              TweenMax.fromTo(fairyConatainer, fairyDuration * 0.2, {
                opacity: 1,
                immediateRender: false
              }, {
                opacity: 0,
                ease: Linear.easeNone,
                delay: fairyDuration * 0.6
              }),
            ]
          }),
          TweenMax.fromTo(fairy, fairyDuration * 0.1 + 0.1 * Math.random(), {
            opacity: 0.7,
            immediateRender: true
          }, {
            opacity: 1,
            ease: Sine.easeInOut,
            yoyo: true,
            repeat: -1
          }),
          TweenMax.fromTo(fairy, fairyDuration * 0.1 + 0.1 * Math.random(), {
            scale: 0.9,
            immediateRender: true
          }, {
            scale: 1,
            ease: Sine.easeInOut,
            yoyo: true,
            repeat: -1
          }),
        ], fiaryDelay);

        fairyConatainer.appendChild(fairy);
        fairiesContainer.appendChild(fairyConatainer);
      }
      var fairyClassElements = [].slice.call(document.getElementsByClassName('fairy'));
      console.log("num fairy elements: ", fairyClassElements.length);

      var resolvePageCTA = document.getElementById('resolve-page-cta');
      resolvePageCTA.addEventListener('mouseenter', function() {
        fairiesTL.restart(true);
      });
      resolvePageCTA.addEventListener('mouseleave', function() {
        fairiesTL.pause();
        fairiesTL.seek(0);
      });

      /** exits **/
      Ad.addExit("Main Exit", "intro-page-exit");
      Ad.addExit("Main Exit", "video-page-exit");
      Ad.addExit("Main Exit", "resolve-page-exit");
      Ad.addExit("Tickets Exit", "video-page-cta");
      Ad.addExit("Tickets Exit", "resolve-page-cta");

      Ad.addTicketsCounter("video-page-cta");
      Ad.addTicketsCounter("resolve-page-cta");
      
      Enabler.setProfileId(1083017);
      var devDynamicContent = {};

      devDynamicContent.Profile= [{}];
      devDynamicContent.Profile[0]._id = 0;
      devDynamicContent.Profile[0].intro = "6mIjIA3_MFc";
      devDynamicContent.Profile[0].video1 = "pvOTvh34FNE";
      Enabler.setDevDynamicContent(devDynamicContent);

      var ctaButtons = document.getElementsByClassName("cta-button");
      var ctaBackgrounds = document.getElementsByClassName("cta-button-background");

      for (var i = 0; i < ctaButtons.length; i++) {
        Utils.addListener(ctaButtons[i], "mouseover", handleButtonFocus, false);
        Utils.addListener(ctaButtons[i], "mouseout", handleButtonBlur, false);
      }

      function handleButtonFocus(event) {
        for (var i = 0; i < ctaBackgrounds.length; i++) {
          Utils.addClass(ctaBackgrounds[i], "over");
          Utils.removeClass(ctaBackgrounds[i], "out");
        }
      }

      function handleButtonBlur(event) {
        for (var i = 0; i < ctaBackgrounds.length; i++) {
          Utils.addClass(ctaBackgrounds[i], "out");
          Utils.removeClass(ctaBackgrounds[i], "over");
        }
      }

      function handleDomContentLoaded(event) {}

      function handleWebComponentsReady(event) {

        setTimeout(function() {
          gwdAd.initAd();
        }, 0);
      }

      function handleAdInitialized(event) {
        /** AVATARLABS SPECIFIC CODE - START **/
        
        var data = dynamicContent.Profile[0];
        if (data.intro != "") YoutubeIntroPlayer.overwriteSource("intro", "Intro", data.intro);
        if (data.video1 != "") YoutubeVideoPlayer.overwriteSource("video1", "Video 1", data.video1);

        YoutubeIntroPlayer.init();
        YoutubeIntroControls.init();

        YoutubeVideoPlayer.init();
        YoutubeVideoControls.init();

        var fairyBGImageURL = Enabler.getUrl("ad_fairies.png");
        console.log("num fairy elements: ", fairyClassElements.length);
        for (i = 0; i < fairyClassElements.length; i++) {
          console.log(fairyClassElements[i], "url('" + fairyBGImageURL + "')");
          fairyClassElements[i].style.backgroundImage = "url('" + fairyBGImageURL + "')";
        }

        Ad.init();

        TrackingController.fireTrackingPixel("initial_load");

        /** desktop  **/
        if (Utils.isMobile()) {
          gwd.handleRequestResolvePage();
        } else {
          gwd.handleRequestIntroPage();
        }
        /** AVATARLABS SPECIFIC CODE - END **/
      }

      window.addEventListener('DOMContentLoaded',
        handleDomContentLoaded, false);
      window.addEventListener('WebComponentsReady',
        handleWebComponentsReady, false);
      window.addEventListener('adinitialized',
        handleAdInitialized, false);
    })();
  </script>


