			}
		}

		for(var i = 0; i<_selectors.length; i++){
			Utils.addListener(_selectors[i].div, "click", requestVideo, false);
		}

		if(_menuButton != undefined){
			Utils.addListener(_menuButton, "mouseover", showMenu, false);
		}
	}

	function requestVideoPlay(event){
		YoutubeIntroPlayer.play();
	}

	function requestVideoPause(event){
		YoutubeIntroPlayer.pause();
	}

	function requestVideoMute(event){
		YoutubeIntroPlayer.mute();
	}

	function requestVideoUnmute(event){
		Debug.log(_debugId, "requestVideoUnmute()");
		YoutubeIntroPlayer.unmute();
	}

	function requestVideo(event){
		var currentTarget = Utils.getEventTarget(event);
		var currentSelector = Utils.getClosest(currentTarget, ".video-selector");
		Debug.log(_debugId, "requestVideo(" + currentSelector.id + ")");
		for(var i = 0; i<_selectors.length; i++){
			if(_selectors[i].div == currentSelector){
				YoutubeIntroPlayer.loadVideo(_selectors[i].id);
			}
		}
	}

	function requestFullscreen(event){
		Debug.log(_debugId, "requestFullscreen()");

		YoutubeIntroPlayer.destroy();

		if(!TrackingController.getUserInteracted()){
			TrackingController.setUserInteracted(true);
			TrackingController.counter("Engaged Video View Ctr");
		}

		FullscreenPlayer.loadVideo(YoutubeIntroPlayer.getVideoType());
	}

	function onMouseDown(event){
		Debug.log(_debugId, "onMouseDown()");

		Utils.removeListener(_scrubberTrigger, "mousedown", onMouseDown);
		Utils.addListener(document, "mousemove", thumbMove, false);
		Utils.addListener(document, "mouseup", onMouseUp, false);
		thumbMove(event);
		Utils.preventDefault(event);
	};

	function onMouseUp(event){
		Debug.log(_debugId, "onMouseUp()");
		
		Utils.addListener(_scrubberTrigger, "mousedown", onMouseDown, false);
		Utils.removeListener(document, "mousemove", thumbMove);
		Utils.removeListener(document, "mouseup", onMouseUp);
		Utils.preventDefault(event);
	};

	function onTouchStart(event){
		Debug.log(_debugId, "onTouchStart()");
		
		Utils.removeListener(_scrubberTrigger, "touchstart", onTouchStart);
		Utils.addListener(document, "touchmove", thumbMove, false);
		Utils.addListener(document, "touchend", onTouchEnd, false);
		thumbMove(event);
		Utils.preventDefault(event);
	};
	
	function onTouchEnd(event){
		Debug.log(_debugId, "onTouchEnd()");

		Utils.addListener(_scrubberTrigger, "touchstart", onTouchStart, false);
		Utils.removeListener(document, "touchmove", thumbMove);
		Utils.removeListener(document, "touchend", onTouchEnd);
		Utils.preventDefault(event);
	};

	function thumbMove(event){
		var scrubberX, _scrubberY;
		if(_videopage){
			scrubberX = _videopage.offsetLeft + _div.offsetLeft + _scrubberContainer.offsetLeft + _scrubberTrigger.offsetLeft;
			scrubberY = _videopage.offsetTop + _div.offsetTop + _scrubberContainer.offsetTop + _scrubberTrigger.offsetTop;
		}else{
			scrubberX = _div.offsetLeft + _scrubberContainer.offsetLeft + _scrubberTrigger.offsetLeft;
			scrubberY = _div.offsetTop + _scrubberContainer.offsetTop + _scrubberTrigger.offsetTop;
		}
		
		var scrubberWidth = _scrubberTrigger.offsetWidth;
		var scrubberHeight =_scrubberTrigger.offsetTop;

		var localX;
		var localY;
		
		if(event.touches)
		{
			localX = event.touches[0].pageX - scrubberX;
			localY = event.touches[0].pageY - scrubberY;
		}
		else
		{
			if(window.event != null){
				localX = event.clientX - scrubberX;
				localY = event.clientY - scrubberY;
			} else {
				localX = event.pageX - scrubberX;
				localY = event.pageY - scrubberY;
			}
		}
		
		var seekPercentage = localX / parseInt(_scrubberTrigger.offsetWidth);
		var seekSeconds = _duration * seekPercentage;

		if(!TrackingController.getUserInteracted()){
			TrackingController.setUserInteracted(true);
			TrackingController.counter("Engaged Video View Ctr");
			if(YoutubeIntroPlayer.getIsPlaying()){
				TrackingController.startTimer("Engaged Video View Timer");
			}
		}

		YoutubeIntroPlayer.seek(seekSeconds);
		if(event) Utils.preventDefault(event);
	}

	function showMenu(event){
		Debug.log(_debugId, "showMenu()");
		if(!_menuIsShowing){
			Utils.addClass(_menuContainer, "video-selector-container-expand");
			setTimeout(function(){
				Utils.addListener(document, "mousemove", evaluateMenu, false);
				Utils.addListener(document, "mouseout", evaluateMenu, false);
			}, 500);
			_menuIsShowing = true;
		}
	}

	function hideMenu(event){
		Debug.log(_debugId, "hideMenu()");
		if(_menuIsShowing){
			Utils.removeClass(_menuContainer, "video-selector-container-expand");
			Utils.removeListener(document, "mousemove", evaluateMenu);
			Utils.removeListener(document, "mouseout", evaluateMenu);
			_menuIsShowing = false;
		}
	}

	function evaluateMenu(event){
		var menuX = _div.offsetLeft + _menuOuter.offsetLeft;
		var menuY = _div.offsetTop + _menuOuter.offsetTop;
		var menuWidth = _menuOuter.offsetWidth;
		var menuHeight =_menuOuter.offsetHeight;

		var buffer = 24 // temp - height of menu plus couple extra pixels

		var localX;
		var localY;
		
		if(event.touches)
		{
			localX = event.touches[0].pageX - menuX;
			localY = event.touches[0].pageY - menuY;
		}
		else
		{
			if(window.event != null){
				localX = event.clientX - menuX;
				localY = event.clientY - menuY;
			} else {
				localX = event.pageX - menuX;
				localY = event.pageY - menuY;
			}
		}

		//Debug.log(_debugId, "menu.localX: " + localX + ", menu.localY: " + localY + ", menuWidth: " + menuWidth + ", menuHeight: " + menuHeight);

		if(localX < 0 || localX > menuWidth || localY < 0 || localY > (menuHeight + buffer)){
			hideMenu();
		}
	}

	function startTimer(){
		Debug.log(_debugId, "startTimer()");
		clearInterval(_controlsInterval);
		_controlsInterval = setInterval(hideControls, _hideTime);
	}

	function stopTimer(){
		Debug.log(_debugId, "stopTimer()");
		clearInterval(_controlsInterval);
	}

	function hideControls(){
		Debug.log(_debugId, "hideControls()");
		clearInterval(_controlsInterval);
		_div.style.display = "none"
		
		Utils.removeListener(_boundsClip, "mouseover", showControls);
		Utils.removeListener(_boundsClip, "mousemove", showControls);
		Utils.removeListener(_boundsClip, "mouseout", startTimer);
		Utils.removeListener(_div, "mouseout", startTimer);

		Utils.addListener(_boundsClip, "mousemove", showControls, false);

		setTimeout(function(){
			Utils.addListener(_boundsClip, "mouseover", showControls, false);
			
		}, 200);
	}

	function showControls(){
		Debug.log(_debugId, "showControls()");
		if(_div.style.display == "none") _div.style.display = "block";
		Utils.removeListener(_boundsClip, "mouseover", showControls);
		Utils.removeListener(_boundsClip, "mousemove", showControls);
		Utils.removeListener(_boundsClip, "mouseout", startTimer);
		Utils.removeListener(_div, "mouseout", startTimer);

		Utils.addListener(_boundsClip, "mouseout", startTimer, false);
		Utils.addListener(_div, "mouseout", startTimer, false);
		startTimer();
	}

	return {
		setDiv:function(value){
			_div = document.getElementById(value);
			Debug.log(_debugId, "controls: " + _div);
		},
		updateDisplay:function(value){
			Debug.log(_debugId, "updateDisplay(" + value + ")");

			if(_playButton != undefined && _pauseButton != undefined){
				if(YoutubeIntroPlayer.getIsPlaying()){
					_playButton.style.display = "none";
					_pauseButton.style.display = "block";
				}else{
					_playButton.style.display = "block";
					_pauseButton.style.display = "none";
				}
			}

			if(_muteButton != undefined && _unmuteButton != undefined){
				if(YoutubeIntroPlayer.getIsMuted()){
					_muteButton.style.display = "none";
					_unmuteButton.style.display = "block";
					if(_audioButton) _audioButton.style.display = "block";
				}else{
					_muteButton.style.display = "block";
					_unmuteButton.style.display = "none";
					if(_audioButton) _audioButton.style.display = "none";
				}
			}

			var index = YoutubeIntroPlayer.getVideoIndex();

			for(var i = 0; i<_selectors.length; i++){
				if(i == index){
					Utils.removeClass(_selectors[i].div, "inactive");
					Utils.addClass(_selectors[i].div, "active");
				}else{
					Utils.removeClass(_selectors[i].div, "active");
					Utils.addClass(_selectors[i].div, "inactive");
				}
			}

			if(_autoHide){
				if(value == "complete"){
					stopTimer();
				}else{
					showControls();
				}
			}
		},
		updateTime:function(time, duration){
			if(_enabled){
				_time = time;
				_duration = duration;
				_progress = Math.round((time/duration)*100);
				if(_progressDisplay) _progressDisplay.style.width = _progress + "%";
			}
		},
		enableControls:function(){
			_enabled = true;
			_div.style.display = "block";
		},
		disableControls:function(){
			_enabled = false;
			if(_progressDisplay) _progressDisplay.style.width = 0;
			if(_audioButton) _audioButton.style.display = "none"
			_div.style.display = "none";
		},
		setButton:function(id, div){
			switch(id){
				case "play":
					_playButton = document.getElementById(div);
					break;
				case "pause":
					_pauseButton = document.getElementById(div);
					break;
				case "mute":
					_muteButton = document.getElementById(div);
					break;
				case "unmute":
					_unmuteButton = document.getElementById(div);
					break;
				case "fullscreen":
					_fullscreenButton = document.getElementById(div);
					break;
				case "audio":
					_audioButton = document.getElementById(div);
					break;
				case "menu":
					_menuButton = document.getElementById(div);
					break;
			}
		},
		setSelector:function(id, div){
			_selectors.push({id:id, div:document.getElementById(div)});
		},
		setScrubberContainer:function(div){
			_scrubberContainer = document.getElementById(div);
		},
		setProgressDisplay:function(div){
			_progressDisplay = document.getElementById(div);
		},
		setScrubberTrigger:function(div){
			_scrubberTrigger = document.getElementById(div);
		},
		setMobileOffset:function(value){
			_mobileOffset = value;
		},
		setBounds:function(div){
			_boundsClip = document.getElementById(div);
		},
		setVideoPage:function(div){
			_videopage = document.getElementById(div);
		},
		setMenuContainer:function(div){
			_menuContainer = document.getElementById(div);
		},
		setMenuOuter:function(div){
			_menuOuter = document.getElementById(div);
		},
		init:function(){
			Debug.log(_debugId, "init()");

			_div.style.display = "none";
			if(_audioButton) _audioButton.style.display = "none";

			if(Utils.isMobile()){
				_muteButton.style.display = "none";
				_unmuteButton.style.display = "none";

				_muteButton = undefined;
				_unmuteButton = undefined;
				_audioButton = undefined;

				if(_scrubberContainer){
					if(_mobileOffset){
						_scrubberContainer.style.width = _mobileOffset;
					}
				}
			}else{
				if(_boundsClip != undefined){
					_autoHide = true;
				}
			}

			YoutubeIntroPlayer.useControls(true);
			addListeners();
		}
	}
})();</script>
  <script>var TrackingController = (function(){
	var _debugId = "TrackingController";
	var _campaignId = "AL001";
	var _separator = " : ";
	var _userInteracted = false;
	var _userExited = false;
	var _trackingPixels = [];
	var _userExitedTimeout;
	function loadImage(id, url){
		var element = new Image();
		element.id = id;

		element.onload = function() {
			onLoadSuccess(this.id);
			this.onload = this.onreadystatechange = null;
		};
		element.onerror = function() {
			onLoadError(this.id);
			this.onload = this.onreadystatechange = null;
		};
		element.onreadystatechange = function() {
			if(this.readyState == 'complete' || this.readyState == "loaded") {
				onLoadSuccess(this.id);
				this.onload = this.onreadystatechange = null;
			}
		};

		element.src = url;
	}
	function onLoadSuccess(id) {
		Debug.log(_debugId, "onLoadSuccess(" + id + ")");
	};
	function onLoadError(id) {
		Debug.log(_debugId, "onLoadError(" + id + ")");
	};
	return {
		setCampaignId:function(value){
			_campaignId = value;
		},
		getCampaignId:function(){
			return _campaignId;
		},
		setSeparator:function(value){
			_separator = value;
		},
		getUserInteracted:function(){
			return _userInteracted;
		},
		setUserInteracted:function(value){
			_userInteracted = true;
		},
		getUserExited:function(){
			return _userExited;
		},
		setUserExited:function(value){
			_userExited = value;
		},
		addTrackingPixel:function(id, value){
			_trackingPixels.push({id:id, url:value});
		},
		fireTrackingPixel:function(id){
			for(var i = 0; i<_trackingPixels.length; i++){
				if(_trackingPixels[i].id == id){
					Debug.log(_debugId, "fireTrackingPixel(" + id + ", " + _trackingPixels[i].url + ")");
					loadImage(_trackingPixels[i].id, _trackingPixels[i].url);
				}
			}
		},
		counter:function(value, cumulative){
			if(cumulative == undefined) cumulative = false;
			Debug.log(_debugId, "TrackingController.counter(" + _campaignId + _separator + value +", " + cumulative +  ")");
			Enabler.counter(_campaignId + _separator + value, cumulative);
		},
		event:function(value){
			Debug.log(_debugId, "TrackingController.event(" + _campaignId + _separator + value +  ")");
			Enabler.counter(_campaignId + _separator + value);
		},
		exit:function(value, url){
			//Debug.log(_debugId, "Enabler.exit(" + _campaignId + _separator + value + ", " + url + ")");
			_userExited = true;
			clearTimeout(_userExitedTimeout);
			_userExitedTimeout = setTimeout(function(){
				_userExited = false;
			}, 1000);
			if(url == undefined){
				Enabler.exit(_campaignId + _separator + value);
			}else{
				Enabler.exitOverride(_campaignId + _separator + value, url);
			}
		},
		startTimer:function(value){
			Enabler.startTimer(_campaignId + _separator + value);
		},
		stopTimer:function(value){
			Enabler.stopTimer(_campaignId + _separator + value);
		}
	}
})();</script>
  <script>var YoutubeIntroPlayer = (function(){
	var _debugId = "YoutubeIntroPlayer";

	var _div;
	var _videoplayer;
	var _videoIndex;
	var _videoIsPlaying = false;
	var _videoIsMuted;
	var _videoIsComplete;
	var _videoDuration;
	var _videoTime;
	var _useControls = false;
	var _videos = [];
	var _videoCanPlay = false;
	var _reportIdentifier;
	var _videoType;
	var _midPointPassed = false;
	var _startMuted = false;
	var _preloader;
	var _isFullscreen = false;
	var _hasNext = false;
	var _timeupdateInterval;
	var _divContainer;
	var _sourceChanged = false;
	var _isMobile = false;
	var _completeCallback;

	function addListeners(){
		Utils.addListener(_videoplayer, "ready", handleCanPlay, false);
		Utils.addListener(_videoplayer, "statechange", handleStateChange, false);

		if(Utils.isMobile()){
			Utils.addListener(_videoplayer, "webkitendfullscreen", handleExitFullscreen, false);
		}
	}

	function removeListeners(){
		Utils.removeListener(_videoplayer, "ready", handleCanPlay);
		Utils.removeListener(_videoplayer, "statechange", handleStateChange);

		if(Utils.isMobile()){
			Utils.removeListener(_videoplayer, "webkitendfullscreen", handleExitFullscreen);
		}
	}

	function handleFullscreenChange(event){
		_isFullscreen = !_isFullscreen;
		Debug.log(_debugId, "handleFullscreenChange(" + _isFullscreen + ")");
		if (_isFullscreen) {
			
		}else{
			if(_completeCallback){
				_completeCallback();
			}
		}
	}

	function handleExitFullscreen(event){
		Debug.log(_debugId, "handleExitFullscreen()");
		if(_completeCallback){
				_completeCallback();
			}
	}

	function handleCanPlay(event){
		Debug.log(_debugId, "handleCanPlay()");
		_videoDuration = _videoplayer.a.getDuration();

		if(!_isMobile){
			YoutubeIntroPlayer.restartVideo();
		}else{
			if(_preloader) _preloader.style.display = "none";
			_divContainer.style.left = "0px";
		}
	}

	function handleStateChange(event){
		var playerState = event.getPlayerState();
		Debug.log(_debugId, "handleStateChange(" + playerState + ")");

	  switch(playerState){ 
	    case studioinnovation.YTPlayer.Events.PLAYING:
	      	handleVideoPlay();        
	    	break;
	    case studioinnovation.YTPlayer.Events.PAUSED:
	      	handleVideoPause();
	    	break;
	    case  studioinnovation.YTPlayer.Events.TIMER_START:
	    	_timeupdateInterval = setInterval(handleTimeupdate, 200);
	    	break;
	    case  studioinnovation.YTPlayer.Events.TIMER_STOP:
	    	clearInterval(_timeupdateInterval);
	    	break;
	    case studioinnovation.YTPlayer.Events.ENDED:
	      	handleVideoComplete();
	    	break;
	  }
	}

	function handleVideoPlay(event){
		Debug.log(_debugId, "handleVideoPlay()");

		if(_sourceChanged){
			_sourceChanged = false;
			if(!_isMobile){
				if(_preloader) _preloader.style.display = "none";
				_divContainer.style.left = "0px";
			}
			if(_useControls) YoutubeIntroControls.enableControls();
		}

		_videoIsPlaying = true;
		_videoIsComplete = false;
		if(_useControls){
			YoutubeIntroControls.updateDisplay("play");
		}
	}

	function handleVideoPause(event){
		Debug.log(_debugId, "handleVideoPause()");
		_videoIsPlaying = false;
		if(_useControls){
			YoutubeIntroControls.updateDisplay("pause");
		}

		if(TrackingController.getUserInteracted()){
			TrackingController.stopTimer("Engaged Video View Timer");
		}

		TrackingController.stopTimer("Intro View Timer");
	}

	function handleVideoComplete(event){
		Debug.log(_debugId, "handleVideoComplete()");
		_videoIsPlaying = false;
		_videoIsComplete = true;

		TrackingController.event(_reportIdentifier + " Complete Ctr");
		TrackingController.stopTimer("Intro View Timer");
		
		if(TrackingController.getUserInteracted()){
			TrackingController.event("Engaged Video View Complete Ctr");
			TrackingController.stopTimer("Engaged Video View Timer");
		}

		if(_useControls){
			YoutubeIntroControls.updateDisplay("complete");
		}

		_divContainer.style.left = "-5000px";
		
		if(_completeCallback){
			_completeCallback();
		}
	}

	function handleTimeupdate(){
		if(_videoplayer) _videoTime = _videoplayer.a.getCurrentTime();
		else clearInterval(_timeupdateInterval);
		
		if(_useControls){
			YoutubeIntroControls.updateTime(_videoTime, _videoDuration);
		}

		if(_videoTime >= _videoDuration/2){
			if(!_midPointPassed){
				_midPointPassed = true;
				TrackingController.event(_reportIdentifier + " Midpoint Ctr");
				if(TrackingController.getUserInteracted()){
					TrackingController.event("Engaged Video View Midpoint Ctr");
				}
			}
		}
	}

	function handleVolumeChange(event){
		var temp = !_videoplayer.isMuted();

		if(_videoIsMuted != temp){
			Debug.log(_debugId, "muted state has changed...");
		}

		if(temp == true){
			_videoIsMuted = true;
			if(_useControls){
				YoutubeIntroControls.updateDisplay("muted");
			}
		}else{
			_videoIsMuted = false;
			if(_useControls){
				YoutubeIntroControls.updateDisplay("unmuted");
			}
		}
	}

	function replaceSource(source){
		for(var i = 0; i<_videos.length; i++){
			if(_videos[i].type == source.type){
				Debug.log(_debugId, "replaceSource(" + _videos[i].type + ", " + source.type + ", " + _videos[i].id + ", " + source.reportIdentifier + ")");
				_videos[i].id = source.reportIdentifier;
				_videos[i].videoId = source.videoId;
			}
		}
	}

	return {
		setVideoplayer:function(value){
			_div = document.getElementById(value);
		},
		getVideoPlayer:function(){
			return _videoplayer;
		},
		setPreloader:function(value){
			_preloader = document.getElementById(value);
		},
		setCompleteCallback:function(value){
			_completeCallback = value;
		},
		setSource:function(type, id, videoId){
			_videos.push({type: type, id:id, videoId: videoId});
		},
		setDivContainer:function(value){
			_divContainer = document.getElementById(value);
		},
		useControls:function(value){
			_useControls = value;
		},
		getIsPlaying:function(){
			return _videoIsPlaying;
		},
		getIsComplete:function(){
			return _videoIsComplete;
		},
		getIsMuted:function(){
			return _videoIsMuted;
		},
		getVideoIndex:function(){
			return _videoIndex;
		},
		getVideoType:function(){
			return _videoType;
		},
		setVideoType:function(value){
			_videoType = value;
		},
		resetVideoType:function(){
			_videoType = undefined;
		},
		getHasNext:function(){
			if (_videoIndex>=_videos.length-1) {
				_hasNext=false;
			} else {
				_hasNext=true;
			}
			
			return _hasNext;
		},
		init:function(){
			Debug.log(_debugId, "init()");
			if(_preloader) _preloader.style.display = "none";
			_isMobile = Utils.isMobile();
		},
		pause:function(){
			Debug.log(_debugId, "pause()");
			TrackingController.counter(_reportIdentifier + " Pause Ctr" );
			_videoplayer.pauseVideo();
		},
		play:function(){
			Debug.log(_debugId, "play()");

			if(!TrackingController.getUserInteracted()){
				TrackingController.setUserInteracted(true);
				TrackingController.event("Engaged Video View Ctr");

				if(_videoIsMuted){
			        YoutubeIntroPlayer.unmute();
			        _videoIsMuted = false;
			        handleVolumeChange();
			    }
			}

			TrackingController.startTimer("Engaged Video View Timer");
			TrackingController.startTimer("Intro View Timer");
			TrackingController.counter(_reportIdentifier + " Unpause");

			_videoplayer.playVideo();
		},
		seek:function(value){
			var target = Math.round(value);
			Debug.log(_debugId, "seek(target: " + target + ", duration: " + _videoDuration + ")");
			_videoplayer.a.seekTo(target, true);
		},
		loadVideo:function(id, muted){
			Debug.log(_debugId, "loadVideo(" + id + ", " + muted + ")");
			if(muted == undefined){
				_startMuted = false;
			}else{
				if(muted == true || muted == "true"){
					_startMuted = true;
				}else{
					_startMuted = false;
				}
			}
			YoutubeIntroPlayer.cueVideo(id);
		},
		cueVideo:function(id){
			Debug.log(_debugId, "cueVideo(" + id + ")");
			YoutubeIntroPlayer.reset();
			YoutubeIntroPlayer.destroy();

			_midPointPassed = false;
			if(_preloader) _preloader.style.display = "block";
			_divContainer.style.left = "-5000px";

			for(var i = 0; i<_videos.length; i++){
				if(_videos[i].type == id){
					Debug.log(_debugId, "videoId: " + _videos[i].videoId);
					_reportIdentifier = (_videos[i].id);
					_videoType = _videos[i].type;

					var player = {
					  'containerId': _div,
					  'videoId': _videos[i].videoId,
					  'videoWidth': "100%",
					  'videoHeight': "100%",
					  'suggestedQuality': "high",//[high,medium,low],
					  'playerVars': {
					  	'html5': 1,
					  	'controls': 0,
					    'autoplay': 0,//[1=enable, 2=disable], 
					    'rel': 0,//[1=enable, 2=disable], /*Show related videos*/
					    'showinfo': 0//[1=enable, 2=disable], /*Show info about the uploader    and video title*/
					  }
					};

					_sourceChanged = true;

					setTimeout(function(){
						_videoplayer = new studioinnovation.YTPlayer(player);
						addListeners();
					}, 200)
					_videoIndex = i;
				}
			}
		},
		restartVideo:function(){
			Debug.log(_debugId, "restartVideo()");
			if(_startMuted == false){
				if(_videoIsMuted){
			        _videoplayer.unMute();
			    }
			    _videoIsMuted = false;
			    if(_useControls) YoutubeIntroControls.updateDisplay("unmuted");

			    if(!TrackingController.getUserInteracted()){
					TrackingController.setUserInteracted(true);
					TrackingController.event("Engaged Video View Ctr");
				}

				TrackingController.startTimer("Engaged Video View Timer");
			}else{
				if(!_videoIsMuted){
					_videoplayer.mute();
				}
				_videoIsMuted = true;
				 if(_useControls) YoutubeIntroControls.updateDisplay("muted");
			}

			_startMuted = false;
			
			TrackingController.startTimer("Intro View Timer");
			TrackingController.event(_reportIdentifier + " Play  Ctr");
		    _videoplayer.playVideo();
		},
		mute:function(){
			Debug.log(_debugId, "mute()");
			TrackingController.counter(_reportIdentifier + " Muted  Ctr");
			_videoplayer.mute();
			handleVolumeChange();
		},
		unmute:function(){
			Debug.log(_debugId, "unmute()");
			TrackingController.counter(_reportIdentifier + " Unmuted  Ctr");
			_videoplayer.unMute();
			handleVolumeChange();

			if(!TrackingController.getUserInteracted()){
				YoutubeIntroPlayer.seek(0);
				TrackingController.setUserInteracted(true);
				TrackingController.event("Engaged Video View Ctr");
				TrackingController.startTimer("Engaged Video View Timer");
			}
		},
		reset:function(){
			Debug.log(_debugId, "reset()");

			if(_videoIsPlaying){
				if(_videoplayer != undefined) _videoplayer.pauseVideo();
			}

			_videoIsPlaying = false;
			
			if(_useControls){ 
				YoutubeIntroControls.disableControls();
			}
		},
		destroy:function(){
			Debug.log(_debugId, "destroy(" + _videoIsPlaying + ")");
			if(_videoplayer){
				if(_videoIsPlaying){
					TrackingController.stopTimer("Intro View Timer");
					if(TrackingController.getUserInteracted()){
						TrackingController.stopTimer("Engaged Video View Timer");
					}

				}

				removeListeners();
				_videoplayer.stopVideo();
				var temp = _videoplayer;

				setTimeout(function(){
					temp.destroy();
				}, 200);
			}
			clearInterval(_timeupdateInterval);
			_videoplayer = undefined;
			_videoIsPlaying = false;
		},
		overwriteSource:function(type, id, videoId){
			Debug.log(_debugId, "overwriteSource(" + type + ", " + id + ", " + videoId + ")");
			for(var i = 0; i<_videos.length; i++){
				if(_videos[i].type == type){
					_videos[i] = {type:type, id:id, videoId:videoId}
				}
			}
		}
	}
})();</script>
  <script>var YoutubeVideoPlayer = (function(){
	var _debugId = "YoutubeVideoPlayer";

	var _div;
	var _videoplayer;
	var _videoIndex;
	var _videoIsPlaying = false;
	var _videoIsMuted;
	var _videoIsComplete;
	var _videoDuration;
	var _videoTime;
	var _useControls = false;
	var _videos = [];
	var _videoCanPlay = false;
	var _reportIdentifier;
	var _videoType;
	var _midPointPassed = false;
	var _startMuted = false;
	var _preloader;
	var _isFullscreen = false;
	var _hasNext = false;
	var _timeupdateInterval;
	var _divContainer;
	var _sourceChanged = false;
	var _isMobile = false;
	var _completeCallback;
	var _divContainerOffset = "0px";

	function addListeners(){
		Utils.addListener(_videoplayer, "ready", handleCanPlay, false);
		Utils.addListener(_videoplayer, "statechange", handleStateChange, false);

		if(Utils.isMobile()){
			Utils.addListener(_videoplayer, "webkitendfullscreen", handleExitFullscreen, false);
		}
	}

	function removeListeners(){
		Utils.removeListener(_videoplayer, "ready", handleCanPlay);
		Utils.removeListener(_videoplayer, "statechange", handleStateChange);

		if(Utils.isMobile()){
			Utils.removeListener(_videoplayer, "webkitendfullscreen", handleExitFullscreen);
		}
	}

	function handleFullscreenChange(event){
		_isFullscreen = !_isFullscreen;
		Debug.log(_debugId, "handleFullscreenChange(" + _isFullscreen + ")");
		if (_isFullscreen) {
			
		}else{
			if(_completeCallback){
				_completeCallback();
			}
		}
	}

	function handleExitFullscreen(event){
		Debug.log(_debugId, "handleExitFullscreen()");
		if(_completeCallback){
				_completeCallback();
			}
	}

	function handleCanPlay(event){
		Debug.log(_debugId, "handleCanPlay()");
		_videoDuration = _videoplayer.a.getDuration();
		
		if(!_isMobile){
			YoutubeVideoPlayer.restartVideo();
		}else{
			if(_preloader) _preloader.style.display = "none";
			_divContainer.style.left = _divContainerOffset;

			if(_useControls){
				YoutubeVideoControls.updateDisplay("complete");
				YoutubeVideoControls.enableControls();
			}
		}
	}

	function handleStateChange(event){
		var playerState = event.getPlayerState();
		Debug.log(_debugId, "handleStateChange(" + playerState + ")");

	  switch(playerState){ 
	    case studioinnovation.YTPlayer.Events.PLAYING:
	      	handleVideoPlay();        
	    	break;
	    case studioinnovation.YTPlayer.Events.PAUSED:
	      	handleVideoPause();
	    	break;
	    case  studioinnovation.YTPlayer.Events.TIMER_START:
	    	_timeupdateInterval = setInterval(handleTimeupdate, 200);
	    	break;
	    case  studioinnovation.YTPlayer.Events.TIMER_STOP:
	    	clearInterval(_timeupdateInterval);
	    	break;
	    case studioinnovation.YTPlayer.Events.ENDED:
	      	handleVideoComplete();
	    	break;
	  }
	}

	function handleVideoPlay(event){
		Debug.log(_debugId, "handleVideoPlay()");

		if(_sourceChanged){
			_sourceChanged = false;
			if(!_isMobile){
				if(_preloader) _preloader.style.display = "none";
				_divContainer.style.left = _divContainerOffset;
				if(_useControls) YoutubeVideoControls.enableControls();
			}
		}

		_videoIsPlaying = true;
		_videoIsComplete = false;
		if(_useControls){
			YoutubeVideoControls.updateDisplay("play");
		}
	}

	function handleVideoPause(event){
		Debug.log(_debugId, "handleVideoPause()");
		_videoIsPlaying = false;
		if(_useControls){
			YoutubeVideoControls.updateDisplay("pause");
		}

		if(TrackingController.getUserInteracted()){
			TrackingController.stopTimer("Engaged Video View Timer");
		}

		TrackingController.stopTimer("Video View Timer");
	}

	function handleVideoComplete(event){
		Debug.log(_debugId, "handleVideoComplete()");
		_videoIsPlaying = false;
		_videoIsComplete = true;

		TrackingController.event(_reportIdentifier + " Complete Ctr");
		TrackingController.stopTimer("Video View Timer");
		
		if(TrackingController.getUserInteracted()){
			TrackingController.event("Engaged Video View Complete Ctr");
			TrackingController.stopTimer("Engaged Video View Timer");
		}

		if(_useControls){
			YoutubeVideoControls.updateDisplay("complete");
		}

		_divContainer.style.left = "-5000px";

		if(_completeCallback){
			_completeCallback();
		}
	}

	function handleTimeupdate(){
		if(_videoplayer) _videoTime = _videoplayer.a.getCurrentTime();
		else clearInterval(_timeupdateInterval);
		if(_useControls){
			YoutubeVideoControls.updateTime(_videoTime, _videoDuration);
		}

		if(_videoTime >= _videoDuration/2){
			if(!_midPointPassed){
				_midPointPassed = true;
				TrackingController.event(_reportIdentifier + " Midpoint Ctr");
				if(TrackingController.getUserInteracted()){
					TrackingController.event("Engaged Video View Midpoint Ctr");
				}
			}
		}
	}

	function handleVolumeChange(event){
		var temp = !_videoplayer.isMuted();

		if(_videoIsMuted != temp){
			Debug.log(_debugId, "muted state has changed...");
		}

		if(temp == true){
			_videoIsMuted = true;
			if(_useControls){
				YoutubeVideoControls.updateDisplay("muted");
			}
		}else{
			_videoIsMuted = false;
			if(_useControls){
				YoutubeVideoControls.updateDisplay("unmuted");
			}
		}
	}

	function replaceSource(source){
		for(var i = 0; i<_videos.length; i++){
			if(_videos[i].type == source.type){
				Debug.log(_debugId, "replaceSource(" + _videos[i].type + ", " + source.type + ", " + _videos[i].id + ", " + source.reportIdentifier + ")");
				_videos[i].id = source.reportIdentifier;
				_videos[i].videoId = source.videoId;
			}
		}
	}

	return {
		setVideoplayer:function(value){
			_div = document.getElementById(value);
		},
		getVideoPlayer:function(){
			return _videoplayer;
		},
		setPreloader:function(value){
			_preloader = document.getElementById(value);
		},
		setCompleteCallback:function(value){
			_completeCallback = value;
		},
		setSource:function(type, id, videoId){
			_videos.push({type: type, id:id, videoId: videoId});
		},
		setDivContainer:function(value){
			_divContainer = document.getElementById(value);
		},
		useControls:function(value){
			_useControls = value;
		},
		getIsPlaying:function(){
			return _videoIsPlaying;
		},
		getIsComplete:function(){
			return _videoIsComplete;
		},
		getIsMuted:function(){
			return _videoIsMuted;
		},
		getVideoIndex:function(){
			return _videoIndex;
		},
		getVideoType:function(){
			return _videoType;
		},
		setVideoType:function(value){
			_videoType = value;
		},
		resetVideoType:function(){
			_videoType = undefined;
		},
		getHasNext:function(){
			if (_videoIndex>=_videos.length-1) {
				_hasNext=false;
			} else {
				_hasNext=true;
			}
			
			return _hasNext;
		},
		init:function(){
			Debug.log(_debugId, "init()");
			if(_preloader) _preloader.style.display = "none";
			_isMobile = Utils.isMobile();
		},
		pause:function(){
			Debug.log(_debugId, "pause()");
			TrackingController.counter(_reportIdentifier + " Pause Ctr");
			_videoplayer.pauseVideo();
		},
		play:function(){
			Debug.log(_debugId, "play()");

			if(!TrackingController.getUserInteracted()){
				TrackingController.setUserInteracted(true);
				TrackingController.event("Engaged Video View Ctr");

				if(_videoIsMuted){
			        YoutubeVideoPlayer.unmute();
			        _videoIsMuted = false;
			        handleVolumeChange();
			    }
			}

			TrackingController.startTimer("Engaged Video View Timer");
			TrackingController.startTimer("Video View Timer");
			TrackingController.counter(_reportIdentifier + " Unpause Ctr");

			_videoplayer.playVideo();
		},
		seek:function(value){
			var target = Math.round(value);
			Debug.log(_debugId, "seek(target: " + target + ", duration: " + _videoDuration + ")");
			_videoplayer.a.seekTo(target, true);
		},
		loadVideo:function(id, muted){
			Debug.log(_debugId, "loadVideo(" + id + ", " + muted + ")");
			if(muted == undefined){
				_startMuted = false;
			}else{
				if(muted == true || muted == "true"){
					_startMuted = true;
				}else{
					_startMuted = false;
				}
			}
			YoutubeVideoPlayer.cueVideo(id);
		},
		cueVideo:function(id){
			Debug.log(_debugId, "cueVideo(" + id + ")");
			YoutubeVideoPlayer.reset();
			YoutubeVideoPlayer.destroy();

			_midPointPassed = false;
			if(_preloader) _preloader.style.display = "block";
			_divContainer.style.left = "-5000px";

			for(var i = 0; i<_videos.length; i++){
				if(_videos[i].type == id){
					Debug.log(_debugId, "videoId: " + _videos[i].videoId);
					_reportIdentifier = (_videos[i].id);
					_videoType = _videos[i].type;

					var player = {
					  'containerId': _div,
					  'videoId': _videos[i].videoId,
					  'videoWidth': "100%",
					  'videoHeight': "100%",
					  'suggestedQuality': "high",//[high,medium,low],
					  'playerVars': {
					  	'html5': 1,
					  	'controls': 0,
					    'autoplay': 0,//[1=enable, 2=disable], 
					    'rel': 0,//[1=enable, 2=disable], /*Show related videos*/
					    'showinfo': 0//[1=enable, 2=disable], /*Show info about the uploader    and video title*/
					  }
					};

					_sourceChanged = true;

					setTimeout(function(){
						_videoplayer = new studioinnovation.YTPlayer(player);
						addListeners();
					}, 200)
					_videoIndex = i;
				}
			}
		},
		restartVideo:function(){
			Debug.log(_debugId, "restartVideo()");
			if(_startMuted == false){
				if(_videoIsMuted){
			        _videoplayer.unMute();
			    }
			    _videoIsMuted = false;
			    if(_useControls) YoutubeVideoControls.updateDisplay("unmuted");

			    if(!TrackingController.getUserInteracted()){
					TrackingController.setUserInteracted(true);
					TrackingController.event("Engaged Video View Ctr");
				}

				TrackingController.startTimer("Engaged Video View Timer");
			}else{
				if(!_videoIsMuted){
					_videoplayer.mute();
				}
				_videoIsMuted = true;
				 if(_useControls) YoutubeVideoControls.updateDisplay("muted");
			}

			_startMuted = false;
			
			TrackingController.startTimer("Video View Timer");
			TrackingController.event(_reportIdentifier + " Play Ctr");
		    _videoplayer.playVideo();
		},
		mute:function(){
			Debug.log(_debugId, "mute()");
			TrackingController.counter(_reportIdentifier + " Muted Ctr");
			_videoplayer.mute();
			handleVolumeChange();
		},
		unmute:function(){
			Debug.log(_debugId, "unmute()");
			TrackingController.counter(_reportIdentifier + " Unmuted Ctr");
			_videoplayer.unMute();
			handleVolumeChange();

			if(!TrackingController.getUserInteracted()){
				YoutubeVideoPlayer.seek(0);
				TrackingController.setUserInteracted(true);
				TrackingController.event("Engaged Video View Ctr");
				TrackingController.startTimer("Engaged Video View Timer");
			}
		},
		reset:function(){
			Debug.log(_debugId, "reset()");

			if(_videoIsPlaying){
				if(_videoplayer != undefined) _videoplayer.pauseVideo();
			}

			_videoIsPlaying = false;
			
			if(_useControls){ 
				YoutubeVideoControls.disableControls();
			}
		},
		destroy:function(){
			Debug.log(_debugId, "destroy(" + _videoIsPlaying + ")");
			if(_videoplayer){
				if(_videoIsPlaying){
					TrackingController.stopTimer("Video View Timer");
					if(TrackingController.getUserInteracted()){
						TrackingController.stopTimer("Engaged Video View Timer");
					}

				}

				removeListeners();
				_videoplayer.stopVideo();
				var temp = _videoplayer;

				setTimeout(function(){
					temp.destroy();
				}, 200);
			}
			clearInterval(_timeupdateInterval);
			_videoplayer = undefined;
			_videoIsPlaying = false;
		},
		overwriteSource:function(type, id, videoId){
			Debug.log(_debugId, "overwriteSource(" + type + ", " + id + ", " + videoId + ")");
			for(var i = 0; i<_videos.length; i++){
				if(_videos[i].type == type){
					_videos[i] = {type:type, id:id, videoId:videoId}
				}
			}
		},
		setDivContainerOffset:function(value){
			_divContainerOffset = value;
		}
	}
})();</script>
  <script>var FullscreenPlayer = (function(){
	var _debugId = "FullscreenPlayer";
	var _isSupported = false;
	var _div;
	var _videos = [];
	var _videoModule;
	var _reportIdentifier;
	var _videoType;
	var _videoplayer;
	var _videoIndex;
	var _videoIsPlaying
	var _videoIsComplete;
	var _videoIsMuted;
	var _completeCallback;
	var _fullscreenCallback;
	var _isFullscreen = false;
	var _container;
	var _useContainer = false;

	function enable(){
		Debug.log(_debugId, "enable()");
	}

	function disable(){
		Debug.log(_debugId, "disable()");
	}

	function addListeners(){
		Debug.log(_debugId, "addListener()");
		Utils.addListener(_videoplayer, "play", handleVideoPlay, false);
		Utils.addListener(_videoplayer, "pause", handleVideoPause, false);
		Utils.addListener(_videoplayer, "ended", handleVideoComplete, false);
		Utils.addListener(_videoplayer, "loadedmetadata", handleMetadata, false);
		Utils.addListener(_videoplayer, "durationchange", handleDurationChange, false);
		Utils.addListener(_videoplayer, "volumechange", handleVolumeChange, false);
		Utils.addListener(_videoplayer, "canplay", handleCanPlay, false);
		
		Utils.addListener(window, "webkitfullscreenchange", handleFullscreenChange, false);
		Utils.addListener(window, "mozfullscreenchange", handleFullscreenChange, false);
		Utils.addListener(window, "MSFullscreenChange", handleFullscreenChange, false);
		Utils.addListener(window, "fullscreenchange", handleFullscreenChange, false);
	}

	function removeListeners(){
		Utils.addListener(_videoplayer, "play", handleVideoPlay, false);
		Utils.addListener(_videoplayer, "pause", handleVideoPause, false);
		Utils.removeListener(_videoplayer, "ended", handleVideoComplete);
		Utils.removeListener(_videoplayer, "loadedmetadata", handleMetadata);
		Utils.removeListener(_videoplayer, "durationchange", handleDurationChange);
		Utils.removeListener(_videoplayer, "volumechange", handleVolumeChange);
		Utils.removeListener(_videoplayer, "canplay", handleCanPlay);
		
		Utils.removeListener(window, "webkitfullscreenchange", handleFullscreenChange);
		Utils.removeListener(window, "mozfullscreenchange", handleFullscreenChange);
		Utils.removeListener(window, "MSFullscreenChange", handleFullscreenChange);
		Utils.removeListener(window, "fullscreenchange", handleFullscreenChange);
	}

	function handleFullscreenChange(event){
		_isFullscreen = !_isFullscreen;
		Debug.log(_debugId, "handleFullscreenChange(" + _isFullscreen + ")");
		if (_isFullscreen) {
			
		}else{
			_container.style.display = "none";
			if(_videoModule){
				 studio.video.Reporter.detach(_reportIdentifier); 
				_videoModule.destroy();
				_videoModule = undefined;
			}
			removeListeners();

			if(_completeCallback){
				_completeCallback();
			}
		}
	}

	function handleCanPlay(event){
		Debug.log(_debugId, "handleCanPlay()");
	}

	function handleVideoPlay(event){
		Debug.log(_debugId, "handleVideoPlay()");

		if(TrackingController.getUserInteracted()){
			TrackingController.startTimer("Engaged Video View Timer");
		}

		TrackingController.startTimer("Fullscreen View Timer");

		_videoIsPlaying = true;
		_videoIsComplete = false;
	}

	function handleVideoPause(event){
		Debug.log(_debugId, "handleVideoPause()");
		_videoIsPlaying = false;

		if(TrackingController.getUserInteracted()){
			TrackingController.stopTimer("Engaged Video View Timer");
		}

		TrackingController.stopTimer("Fullscreen View Timer");
	}

	function handleVideoComplete(event){
		Debug.log(_debugId, "handleVideoComplete()");
		_videoIsPlaying = false;
		_videoIsComplete = true;

		TrackingController.event(_reportIdentifier + " Complete");
		TrackingController.stopTimer("Fullscreen View Timer");
		
		if(TrackingController.getUserInteracted()){
			TrackingController.event("Engaged Video View Complete Ctr");
			TrackingController.stopTimer("Engaged Video View Timer");
		}

		if (document.cancelFullScreen) {
			Debug.log(_debugId, "cancelFullScreen()");
		    document.cancelFullScreen();
		} else if (document.mozCancelFullScreen) {
			Debug.log(_debugId, "mozCancelFullScreen()");
		    document.mozCancelFullScreen(); 
		} else if (document.webkitCancelFullScreen) {
			Debug.log(_debugId, "webkitCancelFullScreen()");
		    document.webkitCancelFullScreen(); 
		}else if(document.msExitFullscreen){
			Debug.log(_debugId, "msExitFullscreen()");
			document.msExitFullscreen();
		}else{
			Debug.log(_debugId, "exitFullscreen()");
			document.exitFullscreen();
		}
	}

	function handleTimeupdate(event){
		_videoTime = _videoplayer.currentTime;

		if(_videoDuration != undefined){
			if(_videoTime >= _videoDuration/2){
				if(!_midPointPassed){
					_midPointPassed = true;
					TrackingController.event(_reportIdentifier + " Midpoint");
					if(TrackingController.getUserInteracted()){
						TrackingController.event("Engaged Video View Midpoint Ctr");
					}
				}
			}
		}
	}

	function handleMetadata(event){
		Debug.log(_debugId, "handleMetadata()");
		_videoDuration = _videoplayer.duration;
		Utils.addListener(_videoplayer, "timeupdate", handleTimeupdate, false);
	}

	function handleDurationChange(event){
		Debug.log(_debugId, "handleDurationChange()");
		_videoDuration = _videoplayer.duration;
		Utils.addListener(_videoplayer, "timeupdate", handleTimeupdate, false);
	}

	function handleVolumeChange(event){
		var currentVolume = _videoplayer.volume;
		_videoIsMuted = _videoplayer.muted;
		Debug.log(_debugId, "handleVolumeChange(" + currentVolume + ", " + _videoIsMuted  + ")");

		if(_videoIsMuted){
			//TrackingController.counter(_reportIdentifier + " Muted");
		}else{
			//TrackingController.counter(_reportIdentifier + " Unmuted");
		}
	}

	function replaceSource(source){
		for(var i = 0; i<_videos.length; i++){
			if(_videos[i].type == source.type){
				Debug.log(_debugId, "replaceSource(" + _videos[i].type + ", " + source.type + ", " + _videos[i].id + ", " + source.reportIdentifier + ")");
				_videos[i].id = source.reportIdentifier;
				_videos[i].mp4 = source.mp4;
				_videos[i].webm = source.webm;
			}
		}
	}

	return {
		isSupported:function(){
			Debug.log(_debugId, "isSupported(" + _isSupported + ")");
			return _isSupported;
		},
		setVideoplayer:function(value){
			_div = document.getElementById(value);
		},
		setContainer:function(value){
			_container = document.getElementById(value);
		},
		setFullscreenType:function(value){
			if(value == "container"){
				_useContainer = true;
			}else{
				_useContainer = false;
			}
		},
		setSource:function(type, id, mp4, webm){
			_videos.push({type: type, id:id, mp4:mp4, webm:webm});
		},
		setCompleteCallback:function(value){
			_completeCallback = value;
		},
		setFullscreenCallback:function(value){
			_fullscreenCallback = value;
		},
		init:function(){
			if (typeof document.cancelFullScreen != 'undefined' || typeof document.webkitCancelFullScreen != 'undefined' || typeof document.mozCancelFullScreen != 'undefined' || typeof document.msExitFullscreen != 'undefined' || typeof document.exitFullscreen != 'undefined') {
			    _isSupported = true;
			}else{
				_isSupported = false;
			}

			Debug.log(_debugId, "init(" + _isSupported + ")");
			
			_isSupported ? enable() : disable();
		},
		loadVideo:function(value){
			var type = value;
			Debug.log(_debugId, "loadVideo(" + type + ")");
			
			if(_fullscreenCallback){
				_fullscreenCallback();
			}

			if(_videoModule){
				 studio.video.Reporter.detach(_reportIdentifier); 
				_videoModule.destroy();
			}

			_container.style.display = "block";

			_midPointPassed = false;

			var videoSources;

			for(var i = 0; i<_videos.length; i++){
				if(_videos[i].type == type){
					_reportIdentifier = (_videos[i].id);
					_videoType = _videos[i].type;
					if(_videos[i].webm != undefined){
						videoSources = [_videos[i].mp4, _videos[i].webm];
					}else{
						videoSources = [_videos[i].mp4];
					}
					_videoModule = new studio.sdk.rad.Video({
						id: _reportIdentifier,
						autoplay: true,
						controls: true,
						sources: videoSources
				    });

			   		_videoModule.setElement(_div);
					_videoplayer = _videoModule.getVideoElement();
					studio.video.Reporter.attach(_reportIdentifier, _videoplayer, false) 
					addListeners();
					_videoIndex = i;
				}
			}

			TrackingController.event(_reportIdentifier + " Play");

			var element;
			if(_useContainer){
				element = _container;
			}else{
				element = _videoplayer;
			}
			
			if (element.requestFullscreen) {
				Debug.log(_debugId, "requestFullscreen()")
			    element.requestFullscreen();
			} else if (element.mozRequestFullScreen) {
				Debug.log(_debugId, "mozRequestFullScreen()");
			    element.mozRequestFullScreen(); 
			} else if (element.webkitRequestFullscreen) {
				Debug.log(_debugId, "webkitRequestFullscreen()");
			    element.webkitRequestFullscreen(); 
			}else if(element.msRequestFullscreen) {
				Debug.log(_debugId, "msRequestFullscreen()");
				element.msRequestFullscreen();
			}else{
				Debug.log(_debugId, "requestFullscreen()");
				element.requestFullscreen();
			}
		},
		overwrite:function(value){
			var remoteVideos = value;

			for(var i = 0; i<remoteVideos.length; i++){
				replaceSource(remoteVideos[i]);
			}
		},
		destroy:function(){
			Debug.log(_debugId, "destroy(" + _videoIsPlaying + ")");
			if(_videoplayer){
				if(_videoIsPlaying){
					TrackingController.stopTimer("Fullscreen View Timer");
					if(TrackingController.getUserInteracted()){
						TrackingController.stopTimer("Engaged Video View Timer");
					}
				}
				removeListeners();
			}
			_videoplayer = undefined;

			if(_videoModule){
				 studio.video.Reporter.detach(_reportIdentifier); 
				_videoModule.destroy();
			}
			_videoModule = undefined;
		},
	}
})();</script>
  <script>var Ad = (function(window, document){

	var _debugId = "Ad";

	var _panels = [];
	var _exits = [];
	var _socialCounter = [];
	var _ticketsCounter = [];

	var _currentPanel;
	var _currentPanelId;
	var _prevPanel;
	var _prevPanelId;
	var _exitCallback;
	var _autoCallback;
	var _isFullscreen = false;
	var _containerClip;
	var _containerWidth;
	var _containerHeight;
	var _containerX;
	var _containerY;

	var _timerFired = false;
	var _timerIsRunning = false;
	var _targetTime = 3000;
	var _timerInterval;
	var _isExpanded = false;
	var _isMobile = false;
	var _canExpand = false;

	var _replayInterval;
	var _replayDelay = 5000;
	var _replayCallback;

	var _videoComponent, _videocontrolsComponent;
	var _introComponent, _introcontrolsComponent;

	var _trackables = [];

/**  
  * ----------------------------------------------*/
	function addListeners(){
		var i;
		
		for(i = 0; i<_panels.length; i++){
			Utils.addListener(_panels[i].div, "pageactivated", handlePagePresenting, false);
			Utils.addListener(_panels[i].div, "pagedeactivated", handlePageDetached, false);
		}

		for(i = 0; i<_exits.length; i++){
			Utils.addListener(_exits[i].div, "click", handleExitClick, false);
		}

		for(i = 0; i<_ticketsCounter.length; i++){
			Utils.addListener(_ticketsCounter[i].div, "click", handleTicketsClick, false);
		}

		for(i = 0; i<_socialCounter.length; i++){
			Utils.addListener(_socialCounter[i].div, "click", handleSocialClick, false);
		}

		Utils.addListener(window, "webkitfullscreenchange", handleFullscreenChange, false);
		Utils.addListener(window, "mozfullscreenchange", handleFullscreenChange, false);
		Utils.addListener(window, "fullscreenchange", handleFullscreenChange, false);

		if(!_isMobile){
			Utils.addListener(_containerClip, "mouseover", startTimer, false);
			Utils.addListener(document, "mouseout", handleMouseOut, false);
		}

		Utils.addListener(Enabler, studio.events.StudioEvent.EXPAND_FINISH, handleAdExpanded, false);
		Utils.addListener(Enabler, studio.events.StudioEvent.COLLAPSE_FINISH, handleAdCollapsed, false);

		Utils.addListener(window, "click", fireTimer, false);

		for(i = 0; i<_trackables.length;  i++){
			Debug.log(_debugId, "trackable: " + _trackables[i].id);
			Utils.addListener(_trackables[i], "click", trackClick, true);
		}
	}

	function trackClick(event){
		Debug.log(_debugId, "trackClick(" + event.target.id + ")");
		TrackingController.fireTrackingPixel("click");
	}

	function handleAdExpanded(event){
		Debug.log(_debugId, "handleAdExpanded()");
		_isExpanded = true;
	}

	function handleAdCollapsed(event){
		Debug.log(_debugId, "handleAdCollapsed()");
		_isExpanded = false;
	}

	function handleFullscreenChange(event){
		_isFullscreen = !_isFullscreen;
		Debug.log(_debugId, "handleFullscreenChange(" + _isFullscreen + ")");
		if(!_isFullscreen){
			if(_currentPanelId == "banner-video-page" || _currentPanelId == "expanded-video-page"){
				if(_videoComponent.getIsPlaying()){
					_videoComponent.pause();
				}
			}

			if(Utils.isMobile()) if(_exitCallback) _exitCallback();
		}
	}

	function handleMouseOut(event){
		stopTimer();
		Utils.preventDefault(event);
	}

	function evaluate(event){

		_containerX = _containerClip.offsetLeft;
		_containerY = _containerClip.offsetTop;
		_containerWidth = _containerClip.offsetWidth;
		_containerHeight =_containerClip.offsetHeight;

		var localX;
		var localY;
		
		if(window.event != null){
			localX = event.clientX - _containerX;
			localY = event.clientY - _containerY;
		} else {
			localX = event.pageX - _containerX;
			localY = event.pageY - _containerY;
		}

		if(localX > _containerWidth || localY > _containerHeight){
			stopTimer();
		}
	}

	function startTimer(){
		if(!_timerFired && !_timerIsRunning) {
			_timerIsRunning = true;
			_timerInterval = setInterval(fireTimer, _targetTime);
			Utils.addListener(document, "mousemove", evaluate, false);
		}
	}

	function stopTimer(){
		Utils.removeListener(document, "mousemove", evaluate);
		clearInterval(_timerInterval);
		_timerIsRunning = false;
	}

	function fireTimer(event){
		Debug.log(_debugId, "fireTimer()");
		Utils.removeListener(window, "click", fireTimer);
		Utils.removeListener(document, "mousemove", evaluate);
		Utils.removeListener(document, "mouseout", handleMouseOut);
		Utils.removeListener(_containerClip, "mouseover", startTimer);
		clearInterval(_timerInterval);

		if(_timerIsRunning || _isMobile){
			_timerFired = true;
			_timerIsRunning = false;
			TrackingController.event("RM Interactive Impression");
		}
	}

	function handlePagePresenting(event){
		if(_currentPanel != undefined){
			_prevPanel = _currentPanel;
			_prevPanelId = _currentPanelId;
		}
		_currentPanel = Utils.getEventTarget(event);
		_currentPanelId = _currentPanel.id;
		Debug.log(_debugId, "handlePagePresenting(prev panel: "  + _prevPanelId + ", next panel: " + _currentPanelId + ")");
	}

	function disableComponents(){
		Debug.log(_debugId, "disableComponents()");
		if(_currentPanelId == "banner-intro-page"){
			if(Animator.getIsRunning()) Animator.stopAll();
			else if(_introComponent){
				if(_introComponent.getIsPlaying()){
					_introComponent.pause();
				}
			}
		}else if(_currentPanelId == "expanded-auto-page"){
			if(Animator.getIsRunning()){
				Animator.stopAll();
			}
			else if(typeof OverlayPlayer != "undefined"){
				OverlayPlayer.destroy();
			}else{
				_introComponent.destroy();
			}
		}else if(_currentPanelId == "banner-video-page" || _currentPanelId == "expanded-video-page"){
			if(_videoComponent.getIsPlaying()){
				_videoComponent.pause();
			}
		}
	}

	function handlePageDetached(event){
		Debug.log(_debugId, "handlePageDetached(prev panel: "  + _prevPanelId + ", next panel: " + _currentPanelId + ")");
		if(_prevPanelId == "banner-video-page" || _prevPanelId == "expanded-video-page"){
			_videoComponent.destroy();
			_videocontrolsComponent.disableControls();
		}
		if(_prevPanelId == "banner-intro-page"){
			if(Animator.getIsRunning()) Animator.stopAll();
			else if(_introComponent) _introComponent.destroy();
		}
		if(_prevPanelId == "expanded-auto-page"){
			if(Animator.getIsRunning()){
				Animator.stopAll();
			}
			else if(typeof OverlayPlayer != "undefined"){
				OverlayPlayer.destroy();
			}else{
				_introComponent.destroy();
			}
		}
		if(_prevPanelId == "fullscreen-page"){
			FullscreenPlayer.destroy();
		}
	}

	function handleExitClick(event){
		if(_currentPanelId != "banner-resolve-page"){
			disableComponents();
			if(_exitCallback) _exitCallback();
		}

		if(_replayInterval){
			clearInterval(_replayInterval);
		}
		
		_currentExit = Utils.getEventTarget(event);

		for(var i = 0; i<_exits.length; i++){
			if(_currentExit == _exits[i].div){
				Debug.log(_debugId, "handleExitClick(" + _currentExit.id + ", " + _exits[i].id + ")");
				if(_exits[i].url != undefined){
					TrackingController.exit(_exits[i].id, _exits[i].url);
				}else{
					TrackingController.exit(_exits[i].id);
				}
			}
		}
	}

	function handleTicketsClick(event){
		_currentTarget = Utils.getEventTarget(event);

		for(var i = 0; i<_ticketsCounter.length; i++){
			if(_currentTarget == _ticketsCounter[i].div){
				Debug.log(_debugId, "handleTicketsClick(" + _currentTarget.id + ", " + _ticketsCounter[i].id + ")");
				TrackingController.counter("Total Tickets Ctr", true);
			}
		}
	}

	function handleSocialClick(event){
		_currentTarget = Utils.getEventTarget(event);

		for(var i = 0; i<_socialCounter.length; i++){
			if(_currentTarget == _socialCounter[i].div){
				Debug.log(_debugId, "handleSocialClick(" + _currentTarget.id + ", " + _socialCounter[i].id + ")");
				TrackingController.counter("Total Social Ctr", true);
			}
		}
	}

	function handleNextVideo(){
		Debug.log(_debugId, "handleNextVideo()");

		if(!_isMobile){
			if(_canExpand){
				if(_currentPanelId == "expanded-resolve-page"){
					if(!VideoPlayer.getIsPlaying() || VideoPlayer.getIsComplete()){
						VideoPlayer.setVideoType("video"+(VideoPlayer.getVideoIndex()+2))
						if(_replayCallback) _replayCallback();
					}
				}
			}else{
				if(_currentPanelId == "banner-resolve-page"){
					if(!VideoPlayer.getIsPlaying() || VideoPlayer.getIsComplete()){
						VideoPlayer.setVideoType("video"+(VideoPlayer.getVideoIndex()+2))
						if(_replayCallback) _replayCallback();
					}
				}
			}
		}

		clearInterval(_replayInterval);
	}

	function findPanelId(){
		if(_currentPanelId == undefined){
			var pages = document.getElementsByClassName('gwd-page');
			if(pages.length > 0){
				for(var i = 0; i<pages.length; i++){
					if(!Utils.hasClass(pages[i], 'gwd-inactive')){
						_currentPanel = pages[i];
						_currentPanelId = pages[i].id;
						Debug.log(_debugId, "_currentPanelId: " + _currentPanelId);
					}
				}
			}
		}
	}

	return {
		setDocName:function(value){
			_docName = value;
		},
		setKeyTab:function(value){
			_keyTab = value;
		},
		setVideoTab:function(value){
			_videoTab = value;
		},
		setExitCallback:function(value){
			_exitCallback = value;
		},
		setContainerClip:function(value){
			_containerClip = document.getElementById(value);
		},
		setCanExpand:function(value){
			_canExpand = value;
		},
		getIsExpanded:function(){
			return _isExpanded;
		},
		getCurrentPanelId:function(){
			return _currentPanelId;
		},
		addPanel:function(id, div){
			var panel = document.getElementById(div);
			Debug.log(_debugId, "addPanel(" + id + ", " + panel.id + ")");
			_panels.push({id: id, div: panel})
		},
		addExit:function(id, div){
			var exit = document.getElementById(div);
			Debug.log(_debugId, "addExit(" + id + ", " + exit.id + ")");
			_exits.push({id: id, div: exit});
		},
		addExitUrl:function(id, url){
			for(var i = 0; i<_exits.length; i++){
				if(_exits[i].id == id){
					_exits[i].url = url;
				}
			}
		},
		addTicketsCounter:function(div){
			var element = document.getElementById(div);
			Debug.log(_debugId, "addTicketsCounter("  + element.id + ")");
			_ticketsCounter.push({id: element.id, div: element});
		},
		addSocialCounter:function(div){
			var element = document.getElementById(div);
			Debug.log(_debugId, "addSocialCounter("  + element.id + ")");
			_socialCounter.push({id: element.id, div: element});
		},
		addTrackables:function(elements){
			for(var i = 0; i<elements.length; i++){
				_trackables.push(elements[i]);
			}
		},
		init:function(){
			Debug.log(_debugId, "init()");

			_isMobile = Utils.isMobile();

			if(typeof IntroPlayer != "undefined"){
				_introComponent = IntroPlayer;
			}else if(typeof YoutubeIntroPlayer != "undefined"){
				_introComponent = YoutubeIntroPlayer;
			}

			if(typeof VideoPlayer != "undefined"){
				_videoComponent = VideoPlayer;
			}else if(typeof YoutubeVideoPlayer != "undefined"){
				_videoComponent = YoutubeVideoPlayer;
			}

			if(typeof VideoControls != "undefined"){
				_videocontrolsComponent = VideoControls;
			}else if(typeof YoutubeVideoControls != "undefined"){
				_videocontrolsComponent = YoutubeVideoControls;
			}

			//hide vide exit button on mobile
			if(_isMobile && typeof YoutubeVideoPlayer != "undefined"){
				for(var i = 0; i<_exits.length; i++){
					if(_exits[i].div.id == "video-page-exit"){
						_exits[i].div.style.display = "none";
					}else if(_exits[i].div.id == "expanded-video-page-exit"){
						_exits[i].div.style.display = "none";
					}
				}
			}
			
			//if gwd started the page before we're adding listeners
			findPanelId();
			addListeners();
		},
		handleRequestExpand:function(){
			if (!TrackingController.getUserInteracted()) {
				TrackingController.setUserInteracted(true);
				TrackingController.event("Engaged Video View Ctr");
			}

			if (_isMobile) {
				_videoComponent.loadVideo("video1");
			} else {
				_videoComponent.resetVideoType();
			}
		},
		handleRequestClose:function(){
			disableComponents();
			if(_replayInterval) clearInterval(_replayInterval);
		},
		handleRequestVideo:function(){
			if (!_isMobile) {
	        	var videoType = _videoComponent.getVideoType();
	        if (videoType == undefined) {
	        	videoType = "video1";
	        }
	        if (!TrackingController.getUserInteracted()) {
	        	_videoComponent.loadVideo(videoType, true);
	        } else {
	        	_videoComponent.loadVideo(videoType, false);
	        }
	      }
		},
		handleRequestIntro:function(){
			Debug.log(_debugId, "handleRequestIntro()");
			if(_introComponent){
				if(TrackingController.getUserInteracted()) {
				   _introComponent.loadVideo("intro", false);
				}else{
				  _introComponent.loadVideo("intro", true);
				}
			}else{
				Animator.startAnimation("Intro-Animation", false, _exitCallback);
			}
		},
		handleRequestOverlay:function(){
			Debug.log(_debugId, "handleRequestOverlay()");
			if(typeof OverlayPlayer != "undefined"){
				if(TrackingController.getUserInteracted()) {
				   OverlayPlayer.loadVideo("overlay", false);
				}else{
				  OverlayPlayer.loadVideo("overlay", true);
				}
			}else{
				Animator.startAnimation("Overlay-Animation", false, _exitCallback);
			}
		},
		handleRequestReplay:function(){
			if (!TrackingController.getUserInteracted()) {
		        TrackingController.setUserInteracted(true);
		        TrackingController.counter("Engaged Video View Ctr");
		    }
			
			if (_isMobile) {
				if(typeof VideoPlayer != "undefined" || typeof YoutubeVideoPlayer != "undefined"){
			        var videoType = _videoComponent.getVideoType();
			        if (videoType == undefined) {
			          videoType = "video1";
			        }
			        _videoComponent.loadVideo(videoType);
			    }else if(_introComponent){
			    	_introComponent.loadVideo("intro", false);
			    }
		    }
		},
		requestNextVideo:function(callback){
			if(!_isMobile){
				_replayCallback = callback;
				Debug.log(_debugId, "has next: " + _videoComponent.getHasNext());
				Debug.log(_debugId, "video is complete: " + _videoComponent.getIsComplete());
				Debug.log(_debugId, "user interacted: " + TrackingController.getUserInteracted());
				if (_videoComponent.getHasNext() && _videoComponent.getIsComplete()) {
					if (TrackingController.getUserInteracted()) {
					  Debug.log(_debugId, "...starting replay timer....");
					  _replayInterval = setInterval(handleNextVideo, _replayDelay)
					}
				}	
			}
		},
		handleRequestCancelAuto:function(){
			if (!TrackingController.getUserInteracted()) {
		        TrackingController.setUserInteracted(true);
		        TrackingController.event("Engaged Video View Ctr");
		    }
		},
		autoExpand:function(id, duration, fn){
			Debug.log(_debugId, "autoExpand(" + id + ", " + duration + ")");

			_autoCallback = fn;
			
			setTimeout(function(){
				Enabler.setStartExpanded(true);
				Enabler.requestExpand();
				
				if(duration != 0){
					setTimeout(function(){
						Debug.log(_debugId, "see if i need to collapse the auto-expand")
						if (!TrackingController.getUserInteracted()) {
							Debug.log(_debugId, "user didn't interact yet");
							if(_isExpanded){
								Debug.log(_debugId, "the ad is expanded");
								 if(_autoCallback != undefined){
								 	Debug.log(_debugId, "found callback..");
								 	if(typeof OverlayPlayer != "undefined"){
								 		if(OverlayPlayer.getIsPlaying){
								 			OverlayPlayer.destroy();
								 		}
								 	}else if(_introComponent){
								 		if(_introComponent.getIsPlaying){
								 			_introComponent.destroy();
								 		}
								 	}else if(Animator.getIsRunning()){
								 		Animator.stopAll();
								 	}
								 	_autoCallback();
								 }
							}
						}
					}, duration)
				}
			}, 0);
		}
	}
})(window, document);</script>
