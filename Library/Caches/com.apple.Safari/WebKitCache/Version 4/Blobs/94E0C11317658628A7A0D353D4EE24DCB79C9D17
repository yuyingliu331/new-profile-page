  <script type="text/javascript" gwd-events="handlers">
    window.gwd = window.gwd || {};
    var isMobile, canExpand, expandPanel, currentPage = '';
    gwd.requestPage = function(value) {
      currentPage = value;
      gwd.actions.gwdDoubleclick.goToPage('gwd-ad', currentPage, 'none', 1000, 'linear', 'top');
    };
    gwd.handleRequestExpand = function(event) {
      Ad.handleRequestExpand();
      gwd.requestPage(expandPanel);
    };
    gwd.handleRequestClose = function() {
      Ad.handleRequestClose();
      gwd.actions.gwdDoubleclick.reportManualClose('gwd-ad');
      gwd.handleRequestBanner();
    };
    gwd.handleRequestRemoval = function() {
      Ad.handleRequestClose();
      setTimeout(function() {
        Enabler.close();
      }, 500);
    };
    gwd.handleFloatingClose = function(event) {
      gwd.actions.gwdDoubleclick.reportManualClose('gwd-ad');
      gwd.handleRequestRemoval();
    };
    gwd.handleAdClosed = function() {
      Debug.log("HTML", "gwd.handleAdClosed()");
      Ad.handleRequestClose();
    }
    gwd.handleRequestIntro = function(event) {
      if (currentPage == '') currentPage = Ad.getCurrentPanelId();
      if (currentPage !== 'banner-intro-page' && currentPage !== "expanded-auto-page") return;
      Ad.handleRequestIntro();
    };
    gwd.handleRequestOverlay = function(event) {
      if (currentPage == '') currentPage = Ad.getCurrentPanelId();
      if (currentPage !== "expanded-auto-page") return;
      Ad.handleRequestOverlay();
    };
    gwd.handleRequestVideo = function(event) {
      if (currentPage == '') currentPage = Ad.getCurrentPanelId();
      if (currentPage !== 'banner-video-page' && currentPage !== 'expanded-video-page') return;
      Ad.handleRequestVideo();
    };
    gwd.handleRequestReplay = function(event) {
      Ad.handleRequestReplay();
      if (typeof VideoPlayer != "undefined" || typeof YoutubeVideoPlayer != "undefined") {
        gwd.handleRequestVideoPage();
      } else {
        gwd.handleRequestIntroPage();
      }
    };
    gwd.handleRequestNext = function(event) {
      Ad.requestNextVideo(gwd.handleRequestVideoPage);
    };
    gwd.handleRequestBanner = function() {
      gwd.requestPage('banner-resolve-page');
    };
    gwd.handleRequestIntroPage = function() {
      gwd.requestPage('banner-intro-page');
    };
    gwd.handleRequestResolvePage = function() {
      gwd.requestPage(canExpand ? 'expanded-resolve-page' : 'banner-resolve-page');
    };
    gwd.handleRequestFullscreenPage = function(event) {
      gwd.requestPage('fullscreen-page');
    };
    gwd.handleRequestVideoPage = function(event) {
      gwd.requestPage(canExpand ? 'expanded-video-page' : 'banner-video-page');
    };
    gwd.handleRequestGalleryPage = function(event) {
      gwd.requestPage(canExpand ? 'expanded-gallery-page' : 'banner-gallery-page');
    };
    gwd.handleRequestAutoExpandPage = function() {
      gwd.requestPage('expanded-auto-page');
    };
    gwd.handleRequestCancelAuto = function(event) {
      Ad.handleRequestCancelAuto();
      gwd.handleRequestVideoPage();
    };
    gwd.dummyFunction = function(event) {
      gwd.actions.gwdDoubleclick.goToPage('gwd-ad', 'banner-resolve-page', 'none', 1000, 'linear', 'top');
    };
    gwd.auto_Dummy_tapareaClick = function(event) {
      // GWD Predefined Function
      gwd.actions.gwdDoubleclick.goToPage('gwd-ad', 'banner-resolve-page', 'none', 1000, 'linear', 'top');
    };
  </script>
  <script type="text/javascript" gwd-events="registration">gwd.actions.events.registerEventHandlers=function(event){gwd.actions.events.addHandler("banner-intro-page","requestIntro",gwd.handleRequestIntro,false);gwd.actions.events.addHandler("banner-video-page","requestVideo",gwd.handleRequestVideo,false);gwd.actions.events.addHandler("resolve-page-replay","click",gwd.handleRequestReplay,false);gwd.actions.events.addHandler("banner-intro-page","close",gwd.handleAdClosed,false);gwd.actions.events.addHandler("banner-video-page","close",gwd.handleAdClosed,false);gwd.actions.events.addHandler("banner-resolve-page","close",gwd.handleAdClosed,false);gwd.actions.events.addHandler("dummy-taparea","click",gwd.auto_Dummy_tapareaClick,false);gwd.actions.timeline.captureAnimationEnd(document.body)};gwd.actions.events.deregisterEventHandlers=function(event){gwd.actions.events.removeHandler("banner-intro-page","requestIntro",gwd.handleRequestIntro,false);gwd.actions.events.removeHandler("banner-video-page","requestVideo",gwd.handleRequestVideo,false);gwd.actions.events.removeHandler("resolve-page-replay","click",gwd.handleRequestReplay,false);gwd.actions.events.removeHandler("banner-intro-page","close",gwd.handleAdClosed,false);gwd.actions.events.removeHandler("banner-video-page","close",gwd.handleAdClosed,false);gwd.actions.events.removeHandler("banner-resolve-page","close",gwd.handleAdClosed,false);gwd.actions.events.removeHandler("dummy-taparea","click",gwd.auto_Dummy_tapareaClick,false);gwd.actions.timeline.releaseAnimationEnd(document.body)};document.addEventListener("DOMContentLoaded",gwd.actions.events.registerEventHandlers);document.addEventListener("unload",gwd.actions.events.deregisterEventHandlers)</script>
