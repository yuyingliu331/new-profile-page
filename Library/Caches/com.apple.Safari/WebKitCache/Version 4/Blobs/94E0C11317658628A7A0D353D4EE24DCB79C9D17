/** css:pointer-events **/
var CssPointerEvents = (function(){
    var _debugId = "CssPointerEvents";
    var _agent = navigator.userAgent;
    var _selector = '*';
    var _mouseEvents = ['click','dblclick','mousedown','mouseup'];
    var _disabledElements;

    function evaluate(){
        if (_agent.match(/MSIE ([0-9]{1,}[\.0-9]{0,})/) != null){
            var version = parseFloat( RegExp.$1 );
            if(version < 11){
                addListeners();
            }
        }
    }
    
    function addListeners(){
        Debug.log(_debugId, "addListeners()");
        _disabledElements = document.getElementsByClassName('disable-pointer');
        for(var i = 0; i<_disabledElements.length; i++){
            _disabledElements[i].style.cursor = "pointer";
            Utils.addListener(_disabledElements[i], "click", handleOnClick, false);
        }
    }

    function handleOnClick(event){
        var target = Utils.getEventTarget(event);
        var origDisplayAttribute = target.style.display;
        target.style.display = "none";
        var underneathElem = document.elementFromPoint(event.clientX, event.clientY);
        if(origDisplayAttribute){
            target.style.display = origDisplayAttribute;
        }else{
            target.style.display = "";
        }
        Utils.dispatchEvent(underneathElem, "click");
    }

    return {
        init:function(){
            Debug.log(_debugId, "init()");
            evaluate();
        }
    }
})();

//testing... make sure Utils and Debug are defined
if(typeof "Utils" != "undefined"){
  Utils.addListener(window, "DOMContentLoaded", function(){Debug.log("PolyFills", "DOMContentLoaded()"); CssPointerEvents.init();}, false);  
}</script>
  <script>var Animator = (function(){
	var _debugId = "Animator";

	var _fps = 22;
	var _animations = [];

	function loadImage(id, source){
		Debug.log(_debugId, "loadImage(" + id + ", " + source + ")");
		var element = new Image();
		element.id = id;

		for(var i = 0; i<_animations.length; i++){
			if(_animations[i].id == id){
				_animations[i].image = element;
			}
		}

		element.onload = function() {
			onLoadSuccess(this.id);
			this.onload = this.onreadystatechange = null;
		};

		element.onerror = function() {
			onLoadError(this.id);
			this.onload = this.onreadystatechange = null;
		};

		element.onreadystatechange = function() {

			if(this.readyState == 'complete' || this.readyState == "loaded") {
				onLoadSuccess(this.id);
				this.onload = this.onreadystatechange = null;
			}
		};

		element.src = source;
	}

	function onLoadSuccess(id) {
		Debug.log(_debugId, "onLoadSuccess(" + id + ")");

		for(var i = 0; i<_animations.length; i++) {
			if(_animations[i].id == id) {
				_animations[i].isLoaded = true;
				_animations[i].spriteWidth = _animations[i].image.width;
				_animations[i].spriteHeight = _animations[i].image.height;

				if(_animations[i].fn != null) {
					_animations[i].fn();
				}
			}
		}
	};

	function onLoadError(id) {
		Debug.log(_debugId, "onLoadError(" + id + ")");
	};

	function animate(obj){
		var element = document.getElementById(obj.div);

		if(obj.currentFrame < obj.totalFrames){

			obj.currentFrame++;

			if(obj.currentXPos >= (obj.spriteWidth-obj.frameWidth)){
				obj.currentXPos = 0;
				obj.currentYPos += obj.frameHeight;
			}else{
				obj.currentXPos += obj.frameWidth;
			}
		}else{
			obj.currentXPos = 0;
			obj.currentYPos = 0;

			if(obj.loop){
				obj.currentFrame = 1;
			}else{
				Debug.log(_debugId, obj.id + " animation done");
				obj.isRunning = false;
				clearInterval(obj.animationLoop);

				if(obj.fn != null){
					obj.fn();
				}

				if(obj.showLastFrame){
					element.style.display = "block";
					obj.showLastFrame = false;
				}else{
					element.style.display = "none";
				}

				return;
			}
		}

		//Debug.log(_debugId,"animate(" + obj.id + " currentFrame: " + obj.currentFrame + ", xPos: " + obj.currentXPos + ", yPos: " + obj.currentYPos + ")");
		var image=element.getElementsByTagName("img")[0];
		image.style.left = -obj.currentXPos + "px";
		image.style.top = -obj.currentYPos + "px";

		if(obj.events.length > 0){
			if(obj.events[obj.currentFrame]){
				obj.events[obj.currentFrame]();
			}
		}
	}

	return {
		setFramesPerSecond:function(value){
			_fps = value;
		},
		getIsRunning:function(){
			var isRunning = false;

			for(var i = 0; i<_animations.length; i++) {
				if(_animations[i].isRunning){
					isRunning = true;
				}
			}

			return isRunning;
		},
		reset:function(){
			for(var i = 0; i<_animations.length; i++){
				var target = _animations[i];
				var element = document.getElementById(target.div);
				element.style.display = "none";
			}
		},
		addRollOver:function(id, divOn, sourceOn, framesOn, divOff, sourceOff, framesOff, width, height){
			Debug.log(_debugId, "addRollOver(" + id + ", " + divOn + ", " + framesOn + ", " + divOff + ", " + framesOff + ", " + width + ", " + height);
			Animator.addAnimation(divOn, divOn, sourceOn, width, height, framesOn, false);
			Animator.addAnimation(divOff, divOff, sourceOff, width, height, framesOff, false);
			var onElement = gwd.actions.events.getElementById(divOn);
			var offElement = gwd.actions.events.getElementById(divOff);
			var element = gwd.actions.events.getElementById(id);
			onElement.rollProps = offElement.rollProps = element.rollProps = {hit:element, id:id, divOn:divOn, framesOn:framesOn, divOff:divOff, framesOff:framesOff, width:width, height:height, over:false};
			Utils.addListener(element, "mouseover", handleMouseOver, false);
			Utils.addListener(element, "mouseout", handleMouseOut, false);
		},
		addAnimation:function(id, div, source, width, height, frames, loop, callback){
			Debug.log(_debugId, "addAnimation(" + id + ", " + div + ", " + source + ", " + width + ", " + height + ", " + frames + ", " + loop +  ")");

			for(var i = 0; i<_animations.length; i++){
				if(_animations[i].id == id){
					Debug.log(_debugId, "animation " + id + " already exists");
					if(_animations[i].isLoaded){
						Debug.log(_debugId, "source already loaded..");
						if(callback){
							Debug.log(_debugId, "found callback..");
							callback();
						}
					}
					return;
				}
			}

			_animations.push({id:id, div:div, src:source, frameWidth:width, frameHeight:height, totalFrames:frames, loop:loop, fn:callback, isLoaded:false, events:[]});
			loadImage(id, source);
		},
		overwriteAnimation:function(id, source, width, height, frames){
			Debug.log(_debugId, "overwriteAnimation(" + id + ", " + source + ", " + width + ", " + height + ", " + frames  + ")");

			for(var i = 0; i<_animations.length; i++){
				if(_animations[i].id == id){
					Debug.log(_debugId, "animation " + id + " found");
					_animations[i].source = source;
					_animations[i].width = width;
					_animations[i].height = height;
					_animations[i].frames = frames;
					loadImage(id, source);
					return;
				}
			}
		},
		addEvent:function(id, frame, callback){
			Debug.log(_debugId, "addEvent(" + id + ", " + frame + ", " + callback + ")");

			for(var i = 0; i<_animations.length; i++){
				if(_animations[i].id == id){
					_animations[i].events[frame] = callback;
				}
			}
		},
		startAnimation:function(id, loop, callback, showLastFrame){
			Debug.log(_debugId, "startAnimation(" + id + ", " + loop + ")");
			for(var i = 0; i<_animations.length; i++){
				if(_animations[i].id == id){
					_animations[i].currentFrame = 1;
					_animations[i].currentXPos = 0;
					_animations[i].currentYPos = 0;
					_animations[i].showLastFrame = showLastFrame;
					_animations[i].loop = loop;
					_animations[i].fn = callback;
					_animations[i].isRunning = true;
					var target = _animations[i];
					var element = document.getElementById(target.div);
					element.style.display = "block";
					var img = element.getElementsByTagName("img")[0];
					img.setAttribute('src', target.src);
					img.style.left = target.currentXPos + "px";
					img.style.top = target.currentYPos + "px";
					target.animationLoop = setInterval(function(){animate(target)}, (1000 / _fps));
				}
			}
		},
		stopAnimation:function(id) {
			for(var i = 0; i<_animations.length; i++){
				if(_animations[i].id == id){
					var target = _animations[i];
					target.showLastFrame = false;
					target.isRunning = false;
					var element = document.getElementById(target.div);
					element.style.display = "none";
					clearInterval(_animations[i].animationLoop);
				}
			}
		},
		resetAnimation:function(id) {
			for(var i = 0; i<_animations.length; i++){
				if(_animations[i].id == id){
					var target = _animations[i];
					var element = document.getElementById(target.div);
					element.style.display = "none";
					var img = element.getElementsByTagName("img")[0];
					img.setAttribute('src', '');
					img.style.left = target.currentXPos + "px";
					img.style.top = target.currentYPos + "px";
				}
			}
		},
		getIsAnimationRunning:function(id){
			var isRunning = false;

			for(var i = 0; i<_animations.length; i++) {
				if(_animations[i].id == id){
					if(_animations[i].isRunning){
						isRunning = true;
					}
				}
			}

			return isRunning;
		},
		stopAll:function(){
			Debug.log(_debugId, "stopAll()");
			for(var i = 0; i<_animations.length; i++){
				var target = _animations[i];
				target.showLastFrame = false;
				target.isRunning = false;
				var element = document.getElementById(target.div);
				element.style.display = "none";
				clearInterval(_animations[i].animationLoop);
			}
		},
		numAnimations:function(){
			return _animations.length;
		}
	}
})();</script>
  <script>var YoutubeVideoControls = (function(){
	var _debugId = "YoutubeVideoControls";
	var _div;
	var _enabled = false;
	var _playButton;
	var _pauseButton;
	var _muteButton;
	var _unmuteButton;
	var _audioButton;
	var _fullscreenButton;
	var _selectors = [];
	var _progressDisplay;
	var _scrubberTrigger;
	var _scrubberContainer;
	var _scrubberHandle;
	var _duration;
	var _time;
	var _mobileOffset;
	var _disableFullscreen;

	var _boundsClip;
	var _controlsAreShowing;
	var _controlsInterval;
	var _autoHide;
	var _hideTime = 4000;
	var _body;
	var _videopage;

	var _menuContainer;
	var _menuOuter;
	var _menuButton;
	var _menuIsShowing = false;

	function addListeners(){

		if(_playButton != undefined){
			Utils.addListener(_playButton, "click", requestVideoPlay, false);
		}
		if(_pauseButton != undefined){
			Utils.addListener(_pauseButton, "click", requestVideoPause, false);
		}
		if(_muteButton != undefined){
			Utils.addListener(_muteButton, "click", requestVideoMute, false);
		}
		if(_unmuteButton != undefined){
			Utils.addListener(_unmuteButton, "click", requestVideoUnmute, false);
		}
		if(_fullscreenButton != undefined){
			Utils.addListener(_fullscreenButton, "click", requestFullscreen, false);
		}
		if(_audioButton != undefined){
			Utils.addListener(_audioButton, "click", requestVideoUnmute, false);
		}
		if(_scrubberTrigger != undefined){
			if ('ontouchstart' in window){
				Utils.addListener(_scrubberTrigger, "touchstart", onTouchStart, false);
			} else {
				Utils.addListener(_scrubberTrigger, "mousedown", onMouseDown, false);
			}
		}

		for(var i = 0; i<_selectors.length; i++){
			Utils.addListener(_selectors[i].div, "click", requestVideo, false);
		}

		if(_menuButton != undefined){
			Utils.addListener(_menuButton, "mouseover", showMenu, false);
		}
	}

	function requestVideoPlay(event){
		YoutubeVideoPlayer.play();
	}

	function requestVideoPause(event){
		YoutubeVideoPlayer.pause();
	}

	function requestVideoMute(event){
		YoutubeVideoPlayer.mute();
	}

	function requestVideoUnmute(event){
		Debug.log(_debugId, "requestVideoUnmute()");
		YoutubeVideoPlayer.unmute();
	}

	function requestVideo(event){
		var currentTarget = Utils.getEventTarget(event);
		var currentSelector = Utils.getClosest(currentTarget, ".video-selector");
		Debug.log(_debugId, "requestVideo(" + currentSelector.id + ")");
		for(var i = 0; i<_selectors.length; i++){
			if(_selectors[i].div == currentSelector){
				YoutubeVideoPlayer.loadVideo(_selectors[i].id);
			}
		}
	}

	function requestFullscreen(event){
		Debug.log(_debugId, "requestFullscreen()");

		YoutubeVideoPlayer.destroy();

		if(!TrackingController.getUserInteracted()){
			TrackingController.setUserInteracted(true);
			TrackingController.counter("Engaged Video View Ctr");
		}

		FullscreenPlayer.loadVideo(YoutubeVideoPlayer.getVideoType());
	}

	function onMouseDown(event){
		Debug.log(_debugId, "onMouseDown()");

		Utils.removeListener(_scrubberTrigger, "mousedown", onMouseDown);
		Utils.addListener(document, "mousemove", thumbMove, false);
		Utils.addListener(document, "mouseup", onMouseUp, false);
		thumbMove(event);
		Utils.preventDefault(event);
	};

	function onMouseUp(event){
		Debug.log(_debugId, "onMouseUp()");
		
		Utils.addListener(_scrubberTrigger, "mousedown", onMouseDown, false);
		Utils.removeListener(document, "mousemove", thumbMove);
		Utils.removeListener(document, "mouseup", onMouseUp);
		Utils.preventDefault(event);
	};

	function onTouchStart(event){
		Debug.log(_debugId, "onTouchStart()");
		
		Utils.removeListener(_scrubberTrigger, "touchstart", onTouchStart);
		Utils.addListener(document, "touchmove", thumbMove, false);
		Utils.addListener(document, "touchend", onTouchEnd, false);
		thumbMove(event);
		Utils.preventDefault(event);
	};
	
	function onTouchEnd(event){
		Debug.log(_debugId, "onTouchEnd()");

		Utils.addListener(_scrubberTrigger, "touchstart", onTouchStart, false);
		Utils.removeListener(document, "touchmove", thumbMove);
		Utils.removeListener(document, "touchend", onTouchEnd);
		Utils.preventDefault(event);
	};

	function thumbMove(event){
		var scrubberX, _scrubberY;
		if(_videopage){
			scrubberX = _videopage.offsetLeft + _div.offsetLeft + _scrubberContainer.offsetLeft + _scrubberTrigger.offsetLeft;
			scrubberY = _videopage.offsetTop + _div.offsetTop + _scrubberContainer.offsetTop + _scrubberTrigger.offsetTop;
		}else{
			scrubberX = _div.offsetLeft + _scrubberContainer.offsetLeft + _scrubberTrigger.offsetLeft;
			scrubberY = _div.offsetTop + _scrubberContainer.offsetTop + _scrubberTrigger.offsetTop;
		}
		
		var scrubberWidth = _scrubberTrigger.offsetWidth;
		var scrubberHeight =_scrubberTrigger.offsetTop;

		var localX;
		var localY;
		
		if(event.touches)
		{
			localX = event.touches[0].pageX - scrubberX;
			localY = event.touches[0].pageY - scrubberY;
		}
		else
		{
			if(window.event != null){
				localX = event.clientX - scrubberX;
				localY = event.clientY - scrubberY;
			} else {
				localX = event.pageX - scrubberX;
				localY = event.pageY - scrubberY;
			}
		}
		
		var seekPercentage = localX / parseInt(_scrubberTrigger.offsetWidth);
		var seekSeconds = _duration * seekPercentage;

		if(_scrubberHandle){
			_scrubberHandle.style.left = (_scrubberTrigger.offsetWidth - _scrubberHandle.offsetWidth) * (seekPercentage) + "px";
		}

		if(!TrackingController.getUserInteracted()){
			TrackingController.setUserInteracted(true);
			TrackingController.counter("Engaged Video View Ctr");
			if(YoutubeVideoPlayer.getIsPlaying()){
				TrackingController.startTimer("Engaged Video View Timer");
			}
		}

		YoutubeVideoPlayer.seek(seekSeconds);
		if(event) Utils.preventDefault(event);
	}

	function showMenu(event){
		Debug.log(_debugId, "showMenu()");
		if(!_menuIsShowing){
			Utils.addClass(_menuContainer, "video-selector-container-expand");
			setTimeout(function(){
				Utils.addListener(document, "mousemove", evaluateMenu, false);
				Utils.addListener(document, "mouseout", evaluateMenu, false);
			}, 500);
			_menuIsShowing = true;
		}
	}

	function hideMenu(event){
		Debug.log(_debugId, "hideMenu()");
		if(_menuIsShowing){
			Utils.removeClass(_menuContainer, "video-selector-container-expand");
			Utils.removeListener(document, "mousemove", evaluateMenu);
			Utils.removeListener(document, "mouseout", evaluateMenu);
			_menuIsShowing = false;
		}
	}

	function evaluateMenu(event){
		var menuX = _div.offsetLeft + _menuOuter.offsetLeft;
		var menuY = _div.offsetTop + _menuOuter.offsetTop;
		var menuWidth = _menuOuter.offsetWidth;
		var menuHeight =_menuOuter.offsetHeight;

		var buffer = 24 // temp - height of menu plus couple extra pixels

		var localX;
		var localY;
		
		if(event.touches)
		{
			localX = event.touches[0].pageX - menuX;
			localY = event.touches[0].pageY - menuY;
		}
		else
		{
			if(window.event != null){
				localX = event.clientX - menuX;
				localY = event.clientY - menuY;
			} else {
				localX = event.pageX - menuX;
				localY = event.pageY - menuY;
			}
		}

		//Debug.log(_debugId, "menu.localX: " + localX + ", menu.localY: " + localY + ", menuWidth: " + menuWidth + ", menuHeight: " + menuHeight);

		if(localX < 0 || localX > menuWidth || localY < 0 || localY > (menuHeight + buffer)){
			hideMenu();
		}
	}

	function startTimer(){
		Debug.log(_debugId, "startTimer()");
		clearInterval(_controlsInterval);
		_controlsInterval = setInterval(hideControls, _hideTime);
	}

	function stopTimer(){
		Debug.log(_debugId, "stopTimer()");
		clearInterval(_controlsInterval);
	}

	function hideControls(){
		Debug.log(_debugId, "hideControls()");
		clearInterval(_controlsInterval);
		_div.style.display = "none"
		
		Utils.removeListener(_boundsClip, "mouseover", showControls);
		Utils.removeListener(_boundsClip, "mousemove", showControls);
		Utils.removeListener(_boundsClip, "mouseout", startTimer);
		Utils.removeListener(_div, "mouseout", startTimer);

		Utils.addListener(_boundsClip, "mousemove", showControls, false);

		setTimeout(function(){
			Utils.addListener(_boundsClip, "mouseover", showControls, false);
			
		}, 200);
	}

	function showControls(){
		Debug.log(_debugId, "showControls()");
		if(_div.style.display == "none") _div.style.display = "block";
		Utils.removeListener(_boundsClip, "mouseover", showControls);
		Utils.removeListener(_boundsClip, "mousemove", showControls);
		Utils.removeListener(_boundsClip, "mouseout", startTimer);
		Utils.removeListener(_div, "mouseout", startTimer);

		Utils.addListener(_boundsClip, "mouseout", startTimer, false);
		Utils.addListener(_div, "mouseout", startTimer, false);
		startTimer();
	}

	return {
		setDiv:function(value){
			_div = document.getElementById(value);
			Debug.log(_debugId, "controls: " + _div);
		},
		updateDisplay:function(value){
			Debug.log(_debugId, "updateDisplay(" + value + ")");

			if(_playButton != undefined && _pauseButton != undefined){
				if(YoutubeVideoPlayer.getIsPlaying()){
					_playButton.style.display = "none";
					_pauseButton.style.display = "block";
				}else{
					_playButton.style.display = "block";
					_pauseButton.style.display = "none";
				}
			}

			if(_muteButton != undefined && _unmuteButton != undefined){
				if(YoutubeVideoPlayer.getIsMuted()){
					_muteButton.style.display = "none";
					_unmuteButton.style.display = "block";
					if(_audioButton) _audioButton.style.display = "block";
				}else{
					_muteButton.style.display = "block";
					_unmuteButton.style.display = "none";
					if(_audioButton) _audioButton.style.display = "none";
				}
			}

			var index = YoutubeVideoPlayer.getVideoIndex();

			for(var i = 0; i<_selectors.length; i++){
				if(i == index){
					Utils.removeClass(_selectors[i].div, "inactive");
					Utils.addClass(_selectors[i].div, "active");
				}else{
					Utils.removeClass(_selectors[i].div, "active");
					Utils.addClass(_selectors[i].div, "inactive");
				}
			}

			if(_autoHide){
				if(value == "complete"){
					stopTimer();
				}else{
					showControls();
				}
			}
		},
		updateTime:function(time, duration){
			if(_enabled){
				_time = time;
				_duration = duration;
				_progress = Math.round((time/duration)*100);
				if(_progressDisplay) _progressDisplay.style.width = _progress + "%";

				if(_scrubberHandle){
					_scrubberHandle.style.left = (_scrubberTrigger.offsetWidth - _scrubberHandle.offsetWidth) * (_progress/100) + "px";
				}
			}
		},
		enableControls:function(){
			_enabled = true;
			_div.style.display = "block";
		},
		disableControls:function(){
			_enabled = false;
			if(_progressDisplay) _progressDisplay.style.width = 0;
			if(_audioButton) _audioButton.style.display = "none"
			_div.style.display = "none";
		},
		setButton:function(id, div){
			switch(id){
				case "play":
					_playButton = document.getElementById(div);
					break;
				case "pause":
					_pauseButton = document.getElementById(div);
					break;
				case "mute":
					_muteButton = document.getElementById(div);
					break;
				case "unmute":
					_unmuteButton = document.getElementById(div);
					break;
				case "fullscreen":
					_fullscreenButton = document.getElementById(div);
					break;
				case "audio":
					_audioButton = document.getElementById(div);
					break;
				case "menu":
					_menuButton = document.getElementById(div);
					break;
			}
		},
		setSelector:function(id, div){
			_selectors.push({id:id, div:document.getElementById(div)});
		},
		setScrubberContainer:function(div){
			_scrubberContainer = document.getElementById(div);
		},
		setProgressDisplay:function(div){
			_progressDisplay = document.getElementById(div);
		},
		setScrubberTrigger:function(div){
			_scrubberTrigger = document.getElementById(div);
		},
		setScrubberHandle:function(div){
			_scrubberHandle = document.getElementById(div);
		},
		setMobileOffset:function(value){
			_mobileOffset = value;
		},
		setBounds:function(div){
			_boundsClip = document.getElementById(div);
		},
		setVideoPage:function(div){
			_videopage = document.getElementById(div);
		},
		setMenuContainer:function(div){
			_menuContainer = document.getElementById(div);
		},
		setMenuOuter:function(div){
			_menuOuter = document.getElementById(div);
		},
		init:function(){
			Debug.log(_debugId, "init()");

			_div.style.display = "none";
			if(_audioButton) _audioButton.style.display = "none";

			Debug.log(_debugId, "mobile? " + Utils.isMobile());
			if(Utils.isMobile()){
				Debug.log(_debugId, "we are on mobile");
				_muteButton.style.display = "none";
				_unmuteButton.style.display = "none";

				_muteButton = undefined;
				_unmuteButton = undefined;
				_audioButton = undefined;

				if(_scrubberContainer){
					Debug.log(_debugId, "found scrubber");
					if(_mobileOffset){
						Debug.log(_debugId, "we have an offset");
						_scrubberContainer.style.width = _mobileOffset + "px";
					}
				}
			}else{
				if(_boundsClip != undefined){
					_autoHide = true;
				}
			}

			YoutubeVideoPlayer.useControls(true);
			addListeners();
		}
	}
})();</script>
  <script>var YoutubeIntroControls = (function(){
	var _debugId = "YoutubeIntroControls";
	var _div;
	var _enabled = false;
	var _playButton;
	var _pauseButton;
	var _muteButton;
	var _unmuteButton;
	var _audioButton;
	var _fullscreenButton;
